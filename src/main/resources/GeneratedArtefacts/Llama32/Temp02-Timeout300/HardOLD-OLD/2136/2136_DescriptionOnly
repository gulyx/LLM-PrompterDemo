package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2136_DescriptionOnly {

    @Test
    public void test1() {
        int[] plantTime = {3,2,3};
        int[] growTime = {4,5,6};
        assertEquals(10, main(plantTime,growTime));
    }

    @Test
    public void test2() {
        int[] plantTime = {1, 2, 3};
        int[] growTime = {1, 2, 3};
        assertEquals(6, main(plantTime,growTime));
    }

    @Test
    public void test3() {
        int[] plantTime = {1, 1, 1};
        int[] growTime = {1, 1, 1};
        assertEquals(3, main(plantTime,growTime));
    }

    @Test
    public void test4() {
        int[] plantTime = {2, 2, 2};
        int[] growTime = {2, 2, 2};
        assertEquals(6, main(plantTime,growTime));
    }

    @Test
    public void test5() {
        int[] plantTime = {1, 3, 2};
        int[] growTime = {4, 3, 1};
        assertEquals(8, main(plantTime,growTime));
    }
}