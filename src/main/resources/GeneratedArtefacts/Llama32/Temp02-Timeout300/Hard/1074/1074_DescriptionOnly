package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertThrows;

public class DescriptionOnly {

    @Test
    public void test1() {
        int[][] matrix = {{0, 1}, {2, -3}};
        long target = 2;
        assertEquals(4, numUniqueSubmatrices(matrix, target));
    }

    @Test
    public void test2() {
        int[][] matrix = {{-5,-6},{1,3}};
        long target = -10;
        assertEquals(0, numUniqueSubmatrices(matrix, target));
    }

    @Test
    public void test3() {
        int[][] matrix = {{1, 2}, {3, 4}};
        long target = 5;
        assertEquals(9, numUniqueSubmatrices(matrix, target));
    }

    @Test
    public void test4() {
        int[][] matrix = {{-10,-11},{12,13}};
        long target = -21;
        assertEquals(0, numUniqueSubmatrices(matrix, target));
    }

    @Test
    public void test5() {
        int[][] matrix = {{1, 2}, {3, 4}};
        long target = 6;
        assertEquals(8, numUniqueSubmatrices(matrix, target));
    }
}