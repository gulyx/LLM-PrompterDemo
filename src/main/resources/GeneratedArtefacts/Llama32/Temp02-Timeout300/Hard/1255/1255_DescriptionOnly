package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 1255_DescriptionOnly {

    @Test
    public void test1() {
        String[] words = {"abc", "cde"};
        int[] letters = {'a', 'b', 'c', 'd'};
        int[] score = {2, 3};
        assertEquals(9, maxScore(words, letters, score));
    }

    @Test
    public void test2() {
        String[] words = {"abc", "cde"};
        int[] letters = {'a', 'b', 'c', 'd'};
        int[] score = {1, 4};
        assertEquals(7, maxScore(words, letters, score));
    }

    @Test
    public void test3() {
        String[] words = {"abc", "cde"};
        int[] letters = {'a', 'b', 'c', 'd'};
        int[] score = {2, 5};
        assertEquals(9, maxScore(words, letters, score));
    }

    @Test
    public void test4() {
        String[] words = {"abc", "cde"};
        int[] letters = {'a', 'b', 'c', 'd'};
        int[] score = {2, 6};
        assertEquals(9, maxScore(words, letters, score));
    }

    @Test
    public void test5() {
        String[] words = {"abc", "cde"};
        int[] letters = {'a', 'b', 'c', 'd'};
        int[] score = {2, 7};
        assertEquals(9, maxScore(words, letters, score));
    }
}