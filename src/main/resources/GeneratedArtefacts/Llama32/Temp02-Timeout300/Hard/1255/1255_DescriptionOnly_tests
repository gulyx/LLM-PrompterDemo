package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1255;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1255.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertThrows;

public class 1255_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String[] words = {"abc", "de", "ef"};
        char[] letters = {'a', 'b', 'c'};
        int[] score = {2, 3, 4};
        assertEquals(10, solution.maxScore(words, letters, score));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String[] words = {"abc", "de", "ef"};
        char[] letters = {'a', 'b'};
        int[] score = {2, 3, 4};
        assertEquals(7, solution.maxScore(words, letters, score));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String[] words = {"abc", "de", "ef"};
        char[] letters = {'a', 'b'};
        int[] score = {2, 4, 5};
        assertEquals(7, solution.maxScore(words, letters, score));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String[] words = {"abc", "de", "ef"};
        char[] letters = {'a', 'b'};
        int[] score = {2, 3};
        assertEquals(5, solution.maxScore(words, letters, score));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String[] words = {"abc", "de", "ef"};
        char[] letters = {'a', 'b'};
        int[] score = {2};
        assertEquals(2, solution.maxScore(words, letters, score));
    }
}