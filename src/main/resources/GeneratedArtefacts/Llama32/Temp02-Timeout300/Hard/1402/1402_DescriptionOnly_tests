package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1402_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] satisfaction = {1, -1, 3, -3};
        assertEquals(2, maxSatisfaction(satisfaction));
    }

    @Test
    public void test2() {
        int[] satisfaction = {-10, -5, 0, 50};
        assertEquals(100, maxSatisfaction(satisfaction));
    }

    @Test
    public void test3() {
        int[] satisfaction = {1, 2, 3, 4, 5};
        assertEquals(15, maxSatisfaction(satisfaction));
    }

    @Test
    public void test4() {
        int[] satisfaction = {-1000, -500, 0, 500, 1000};
        assertEquals(2000, maxSatisfaction(satisfaction));
    }

    @Test
    public void test5() {
        int[] satisfaction = {1, -2, 3, -4, 5};
        assertEquals(9, maxSatisfaction(satisfaction));
    }
}