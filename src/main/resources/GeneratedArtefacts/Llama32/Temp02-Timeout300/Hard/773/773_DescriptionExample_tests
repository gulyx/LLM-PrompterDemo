```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class SolutionTest {

    @Test
    public void testExample1() {
        Solution solution = new Solution();
        int[][] board = {{1,2,3},{4,0,5}};
        assertEquals(1, solution.minMovesToTransform(board));
    }

    @Test
    public void testExample2() {
        Solution solution = new Solution();
        int[][] board = {{1,2,3},{5,4,0}};
        assertEquals(-1, solution.minMovesToTransform(board));
    }

    @Test
    public void testExample3() {
        Solution solution = new Solution();
        int[][] board = {{4,1,2},{5,0,3}};
        assertEquals(5, solution.minMovesToTransform(board));
    }

    @Test
    public void testExample4() {
        Solution solution = new Solution();
        int[][] board = {{1,2,3},{4,5,6}};
        assertEquals(-1, solution.minMovesToTransform(board));
    }

    @Test
    public void testExample5() {
        Solution solution = new Solution();
        int[][] board = {{0,1,2},{3,4,5}};
        assertEquals(10, solution.minMovesToTransform(board));
    }
}
```