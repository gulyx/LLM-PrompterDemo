package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2136;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2136.Solution;

public class 2136_DescriptionOnly_testsTest {

    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.descriptionOnlyTests());
    }
}

import org.junit.Test;
import java.util.Arrays;

public class TestSolution {

    @Test
    public void testDescriptionOnlyTests_1() {
        int[] plantTime = {3, 2};
        int[] growTime = {1, 2};
        int expected = 6;
        Solution solution = new Solution();
        int actual = solution.descriptionOnlyTests(plantTime, growTime);
        assert actual == expected : "Expected " + expected + ", but got " + actual;
    }

    @Test
    public void testDescriptionOnlyTests_2() {
        int[] plantTime = {1, 3};
        int[] growTime = {2, 1};
        int expected = 8;
        Solution solution = new Solution();
        int actual = solution.descriptionOnlyTests(plantTime, growTime);
        assert actual == expected : "Expected " + expected + ", but got " + actual;
    }

    @Test
    public void testDescriptionOnlyTests_3() {
        int[] plantTime = {2, 1};
        int[] growTime = {4, 3};
        int expected = 10;
        Solution solution = new Solution();
        int actual = solution.descriptionOnlyTests(plantTime, growTime);
        assert actual == expected : "Expected " + expected + ", but got " + actual;
    }

    @Test
    public void testDescriptionOnlyTests_4() {
        int[] plantTime = {1};
        int[] growTime = {3};
        int expected = 4;
        Solution solution = new Solution();
        int actual = solution.descriptionOnlyTests(plantTime, growTime);
        assert actual == expected : "Expected " + expected + ", but got " + actual;
    }

    @Test
    public void testDescriptionOnlyTests_5() {
        int[] plantTime = {1};
        int[] growTime = {3};
        int expected = 4;
        Solution solution = new Solution();
        int actual = solution.descriptionOnlyTests(plantTime, growTime);
        assert actual == expected : "Expected " + expected + ", but got " + actual;
    }
}