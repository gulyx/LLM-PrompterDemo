```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1579;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1579.Solution;

public class DescriptionExampleTestsTest {

    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.maxNumEdgesToRemove(4, new int[][]{{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}})); // 2
        System.out.println(solution.maxNumEdgesToRemove(4, new int[][]{{3,1,2},{3,2,3},{1,1,4},{2,1,4}})); // 0
        System.out.println(solution.maxNumEdgesToRemove(4, new int[][]{{3,2,3},{1,1,2},{2,3,4}})); // -1
        System.out.println(solution.maxNumEdgesToRemove(5, new int[][]{{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}})); // 0
        System.out.println(solution.maxNumEdgesToRemove(4, new int[][]{{3,1,2},{3,2,3},{1,1,4},{2,1,4}})); // 0
    }
}
```