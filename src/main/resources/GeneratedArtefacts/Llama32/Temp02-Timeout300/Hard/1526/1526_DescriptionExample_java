package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1526;

public class Solution {
    public int minOperations(int[] target) {
        int operations = 0;
        for (int i = 0; i < target.length; i++) {
            if (target[i] > 0) {
                operations += target[i];
            }
        }
        return operations;
    }

    [--Example 1--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        int[] target = {1,2,3,2,1};
        System.out.println(solution.minOperations(target)); // Output: 3
    }

    [--Example 2--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        int[] target = {3,1,1,2};
        System.out.println(solution.minOperations(target)); // Output: 4
    }

    [--Example 3--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        int[] target = {3,1,5,4,2};
        System.out.println(solution.minOperations(target)); // Output: 7
    }
}