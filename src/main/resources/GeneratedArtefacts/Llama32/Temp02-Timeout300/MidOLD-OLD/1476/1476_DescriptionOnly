package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class SubrectangleQueries {
    private int[][] rectangle;

    public SubrectangleQueries(int[][] rectangle) {
        this.rectangle = rectangle;
    }

    @Test
    void updateSubrectangle() {
        SubrectangleQueries obj = new SubrectangleQueries(new int[][]{{1, 2}, {3, 4}});
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }

    @Test
    void updateSubrectangle_2() {
        SubrectangleQueries obj = new SubrectangleQueries(new int[][]{{1, 2}, {3, 4}});
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }

    @Test
    void updateSubrectangle_3() {
        SubrectangleQueries obj = new SubrectangleQueries(new int[][]{{1, 2}, {3, 4}});
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }

    @Test
    void updateSubrectangle_4() {
        SubrectangleQueries obj = new SubrectangleQueries(new int[][]{{1, 2}, {3, 4}});
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }

    @Test
    void updateSubrectangle_5() {
        SubrectangleQueries obj = new SubrectangleQueries(new int[][]{{1, 2}, {3, 4}});
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }
}