package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1605_DescriptionOnly {

    @Test
    public void test_1() {
        int[][] rowSum = {{15},{12},{10}};
        int[][] colSum = {{13},{11},{14}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(3, result[0].length);
    }

    @Test
    public void test_2() {
        int[][] rowSum = {{1, 1}, {1, 1}};
        int[][] colSum = {{2}, {2}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test_3() {
        int[][] rowSum = {{1, 2}, {3, 4}};
        int[][] colSum = {{5}, {6}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test_4() {
        int[][] rowSum = {{1, 1, 1}, {1, 1, 1}};
        int[][] colSum = {{3}, {3}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(3, result[0].length);
    }

    @Test
    public void test_5() {
        int[][] rowSum = {{1, 2}, {3, 4}};
        int[][] colSum = {{6}, {8}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }
}