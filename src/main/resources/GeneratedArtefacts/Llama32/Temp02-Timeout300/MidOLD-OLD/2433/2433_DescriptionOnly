package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2433_DescriptionOnly {

    @Test
    public void test_XOR_Satisfies_Pref() {
        int[] pref = {1, 2, 4};
        int[] arr = new int[pref.length];
        for (int i = 0; i < pref.length; i++) {
            arr[i] = pref[0]^pref[1]^...^pref[i];
        }
        assertArrayEquals(pref, arr);
    }

    @Test
    public void test_XOR_Satisfies_Pref_2() {
        int[] pref = {3, 5, 9};
        int[] arr = new int[pref.length];
        for (int i = 0; i < pref.length; i++) {
            arr[i] = pref[0]^pref[1]^...^pref[i];
        }
        assertArrayEquals(pref, arr);
    }

    @Test
    public void test_XOR_Satisfies_Pref_3() {
        int[] pref = {7, 10, 5};
        int[] arr = new int[pref.length];
        for (int i = 0; i < pref.length; i++) {
            arr[i] = pref[0]^pref[1]^...^pref[i];
        }
        assertArrayEquals(pref, arr);
    }

    @Test
    public void test_XOR_Satisfies_Pref_4() {
        int[] pref = {2, 1, 1};
        int[] arr = new int[pref.length];
        for (int i = 0; i < pref.length; i++) {
            arr[i] = pref[0]^pref[1]^...^pref[i];
        }
        assertArrayEquals(pref, arr);
    }

    @Test
    public void test_XOR_Satisfies_Pref_5() {
        int[] pref = {6, 5, 4};
        int[] arr = new int[pref.length];
        for (int i = 0; i < pref.length; i++) {
            arr[i] = pref[0]^pref[1]^...^pref[i];
        }
        assertArrayEquals(pref, arr);
    }

}