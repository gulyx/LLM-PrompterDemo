package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2785_DescriptionOnly {

    @Test
    public void test1() {
        String s = "leetcode";
        String t = "leetcod";
        assertEquals(s, permuteString(s));
    }

    @Test
    public void test2() {
        String s = "aabb";
        String t = "abba";
        assertEquals(s, permuteString(s));
    }

    @Test
    public void test3() {
        String s = "aa";
        String t = "aa";
        assertEquals(s, permuteString(s));
    }

    @Test
    public void test4() {
        String s = "bcab";
        String t = "cab";
        assertEquals(s, permuteString(s));
    }

    @Test
    public void test5() {
        String s = "aaab";
        String t = "aabaa";
        assertEquals(s, permuteString(s));
    }
}