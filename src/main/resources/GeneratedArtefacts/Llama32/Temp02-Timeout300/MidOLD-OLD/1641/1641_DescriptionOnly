package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertThrows;

public class DescriptionOnly {

    @Test
    public void test_1() {
        int n = 2;
        assertEquals(6, numVowelStrings(n));
    }

    @Test
    public void test_2() {
        int n = 10;
        assertEquals(1024, numVowelStrings(n));
    }

    @Test
    public void test_3() {
        int n = 15;
        assertEquals(32768, numVowelStrings(n));
    }

    @Test
    public void test_4() {
        int n = 20;
        assertEquals(1048576, numVowelStrings(n));
    }

    @Test
    public void test_5() {
        assertThrows(IllegalArgumentException.class, () -> numVowelStrings(-1));
    }
}