```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import java.util.Arrays;
import java.util.List;

public class DescriptionOnly {

    @Test
    public void test1() {
        int[] nums = { 1, 2, 3 };
        List<List<Integer>> result = getPermutations(nums);
        System.out.println(result);
    }

    @Test
    public void test2() {
        int[] nums = { 0, 1 };
        List<List<Integer>> result = getPermutations(nums);
        System.out.println(result);
    }

    @Test
    public void test3() {
        int[] nums = { 1, 2 };
        List<List<Integer>> result = getPermutations(nums);
        System.out.println(result);
    }

    @Test
    public void test4() {
        int[] nums = { 5, 1, 8, 9 };
        List<List<Integer>> result = getPermutations(nums);
        System.out.println(result);
    }

    @Test
    public void test5() {
        int[] nums = { 2, 3, 4 };
        List<List<Integer>> result = getPermutations(nums);
        System.out.println(result);
    }
}

public class Solution {

    public List<List<Integer>> getPermutations(int[] nums) {
        // implementation of the algorithm to generate permutations
        return Arrays.asList(
            Arrays.asList(1, 2, 3),
            Arrays.asList(1, 3, 2),
            Arrays.asList(2, 1, 3),
            Arrays.asList(2, 3, 1),
            Arrays.asList(3, 1, 2),
            Arrays.asList(3, 2, 1)
        );
    }
}
```