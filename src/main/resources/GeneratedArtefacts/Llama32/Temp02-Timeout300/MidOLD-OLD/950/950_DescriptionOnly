package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 950_DescriptionOnly {

    @Test
    public void test1() {
        int[] deck = {2,4,1};
        String result = "1124";
        assertEquals(result, solution(deck));
    }

    @Test
    public void test2() {
        int[] deck = {5,4,6,2,3,8};
        String result = "6435282";
        assertEquals(result, solution(deck));
    }

    @Test
    public void test3() {
        int[] deck = {1, 1000, 2, 500, 30};
        String result = "150250230";
        assertEquals(result, solution(deck));
    }

    @Test
    public void test4() {
        int[] deck = {7, 6, 5, 4, 3, 1, 10, 8, 2};
        String result = "163457810";
        assertEquals(result, solution(deck));
    }

    @Test
    public void test5() {
        int[] deck = {};
        String result = "";
        assertEquals(result, solution(deck));
    }
}