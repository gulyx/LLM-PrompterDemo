package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class 2391_DescriptionExample {

    @Test
    public void test_1() {
        String[] garbage = {"G", "P", "GP", "GG"};
        int[] travel = {2, 4, 3};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_2() {
        String[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        assertEquals(37, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_3() {
        String[] garbage = {"G", "P", "P", "G"};
        int[] travel = {2, 4, 1, 5};
        assertEquals(15, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_4() {
        String[] garbage = {"M","MP","GG"};
        int[] travel = {3, 10, 2};
        assertEquals(13, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_5() {
        String[] garbage = {"G", "P", "PG", "GP"};
        int[] travel = {2, 4, 3, 1};
        assertEquals(19, minGarbagePickup(garbage, travel));
    }
}