import static org.junit.jupiter.api.Assertions.*;

public class 2396_DescriptionOnly_testsTest {

    @Test
    public void testIsStrictlyPalindromic_1() {
        Solution solution = new Solution();
        assertTrue(solution.isStrictlyPalindromic(1));
    }

    @Test
    public void testIsStrictlyPalindromic_2() {
        Solution solution = new Solution();
        assertTrue(solution.isStrictlyPalindromic(2));
    }

    @Test
    public void testIsStrictlyPalindromic_3() {
        Solution solution = new Solution();
        assertFalse(solution.isStrictlyPalindromic(10));
    }

    @Test
    public void testIsStrictlyPalindromic_4() {
        Solution solution = new Solution();
        assertTrue(solution.isStrictlyPalindromic(9));
    }

    @Test
    public void testIsStrictlyPalindromic_5() {
        Solution solution = new Solution();
        assertFalse(solution.isStrictlyPalindromic(11));
    }
}