    public void test_3() {
        int n = 10;
        boolean result = isStrictlyPalindromic(n);
        assertFalse(result);
    }

    @Test
    public void test_4() {
        int n = 12;
        boolean result = isStrictlyPalindromic(n);
        assertTrue(result);
    }

    @Test
    public void test_5() {
        int n = 12321;
        boolean result = isStrictlyPalindromic(n);
        assertTrue(result);
    }
