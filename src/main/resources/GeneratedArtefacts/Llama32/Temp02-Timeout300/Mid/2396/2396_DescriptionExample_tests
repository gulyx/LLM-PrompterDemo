public class DescriptionExample_testsTest {

    @Test
    public void testIsStrictlyPalindromic_9() {
        Solution solution = new Solution();
        boolean result = solution.isStrictlyPalindromic(9);
        assertFalse(result);
    }

    @Test
    public void testIsStrictlyPalindromic_4() {
        Solution solution = new Solution();
        boolean result = solution.isStrictlyPalindromic(4);
        assertFalse(result);
    }

    @Test
    public void testIsStrictlyPalindromic_10() {
        Solution solution = new Solution();
        boolean result = solution.isStrictlyPalindromic(10);
        assertTrue(result);
    }

    @Test
    public void testIsStrictlyPalindromic_100() {
        Solution solution = new Solution();
        boolean result = solution.isStrictlyPalindromic(100);
        assertTrue(result);
    }

    @Test
    public void testIsStrictlyPalindromic_1() {
        Solution solution = new Solution();
        boolean result = solution.isStrictlyPalindromic(1);
        assertTrue(result);
    }
}
