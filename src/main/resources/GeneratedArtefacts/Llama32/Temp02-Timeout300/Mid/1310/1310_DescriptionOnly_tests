package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1310_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] arr = {1,3};
        int[][] queries = {{0,1}};
        int[] answer = xorQueries(arr, queries);
        assertArrayEquals(new int[]{2}, answer);
    }

    @Test
    public void test2() {
        int[] arr = {1, 2, 4};
        int[][] queries = {{0,1},{1,2}};
        int[] answer = xorQueries(arr, queries);
        assertArrayEquals(new int[]{3, 6}, answer);
    }

    @Test
    public void test3() {
        int[] arr = {1, 2, 4};
        int[][] queries = {{0,2},{1,2}};
        int[] answer = xorQueries(arr, queries);
        assertArrayEquals(new int[]{7,6}, answer);
    }

    @Test
    public void test4() {
        int[] arr = {1, 2, 3, 4};
        int[][] queries = {{0,1},{1,2},{0,2}};
        int[] answer = xorQueries(arr, queries);
        assertArrayEquals(new int[]{3,6,12}, answer);
    }

    @Test
    public void test5() {
        int[] arr = {1, 2, 4};
        int[][] queries = {{0,3},{1,3}};
        int[] answer = xorQueries(arr, queries);
        assertArrayEquals(new int[]{7,6}, answer);
    }

}