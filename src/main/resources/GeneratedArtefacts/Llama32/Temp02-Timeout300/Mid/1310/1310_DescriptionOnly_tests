package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1310_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] arr = {1,3};
        int[][] queries = {{0,1},{1,2}};
        int[] answer = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{2, 4}, answer);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] arr = {1,2,3};
        int[][] queries = {{0,1},{1,2}};
        int[] answer = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{1, 3}, answer);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] arr = {1,2,3};
        int[][] queries = {{0,0},{1,1}};
        int[] answer = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{1, 1}, answer);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] arr = {1,2,3};
        int[][] queries = {{0,2},{1,2}};
        int[] answer = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{3, 3}, answer);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] arr = {1,2,3};
        int[][] queries = {{0,2},{0,2}};
        int[] answer = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{3, 3}, answer);
    }
}