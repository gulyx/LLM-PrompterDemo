```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2326;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2326.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExample_testsTest {

    @Test
    public void testSpiralOrder() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(3, 5, new int[]{3,0,2,6,8,1,7,9,4,2,5,5,0});
        assertArrayEquals(new int[][]{{-1,-1,-1,0,2},{-1,0,1,2,0},{0,1,2,2,1}}, result);
    }

    @Test
    public void testSpiralOrder_2() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(4, 5, new int[]{3,0,2,6,8,1,7,9,4,2,5,5,0});
        assertArrayEquals(new int[][]{{-1,-1,0,0,2},{-1,0,1,2,0},{0,1,2,2,1},{-1,0,1,2,3}}, result);
    }

    @Test
    public void testSpiralOrder_3() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(5, 6, new int[]{3,0,2,6,8,1,7,9,4,2,5,5,0});
        assertArrayEquals(new int[][]{{-1,-1,-1,0,0,2},{-1,0,1,2,0,0},{0,1,2,2,1,0},{0,0,1,2,3,1},{-1,0,1,2,3,4}}, result);
    }

    @Test
    public void testSpiralOrder_4() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(7, 8, new int[]{3,0,2,6,8,1,7,9,4,2,5,5,0});
        assertArrayEquals(new int[][]{{-1,-1,-1,-1,0,0,2},{-1,0,1,2,0,0,0},{0,1,2,2,1,0,0},{0,0,1,2,3,1,0},{0,0,0,1,2,3,1},{-1,0,1,2,3,4,0},{0,-1,0,1,2,3,4}}, result);
    }

    @Test
    public void testSpiralOrder_5() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(8, 9, new int[]{3,0,2,6,8,1,7,9,4,2,5,5,0});
        assertArrayEquals(new int[][]{{-1,-1,-1,-1,-1,0,0,2},{-1,0,1,2,0,0,0,0},{0,1,2,2,1,0,0,0},{0,0,1,2,3,1,0,0},{0,0,0,1,2,3,1,0},{0,0,0,0,1,2,3,1},{-1,0,1,2,3,4,0,0},{0,-1,0,1,2,3,4,0}}, result);
    }
}
```