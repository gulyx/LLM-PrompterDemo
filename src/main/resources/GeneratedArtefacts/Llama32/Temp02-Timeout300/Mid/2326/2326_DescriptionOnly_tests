```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2326;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2326.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionOnly_testsTest {

    @Test
    public void testGenerateMatrix_2x3() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(2, 3);
        assertEquals(new int[][]{{1, 2}, {8, 9}}, result);
    }

    @Test
    public void testGenerateMatrix_3x3() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(3, 3);
        assertEquals(new int[][]{{1, 2, 3}, {8, 9, 4}, {7, 6, 5}}, result);
    }

    @Test
    public void testGenerateMatrix_1x1() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(1, 1);
        assertEquals(new int[][]{{1}}, result);
    }

    @Test
    public void testGenerateMatrix_2x2() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(2, 2);
        assertEquals(new int[][]{{1, 2}, {4, 5}}, result);
    }

    @Test
    public void testGenerateMatrix_0x3() {
        Solution solution = new Solution();
        int[][] result = solution.generateMatrix(0, 3);
        assertArrayEquals(new int[][]{{-1, -1, -1}}, result);
    }
}
```