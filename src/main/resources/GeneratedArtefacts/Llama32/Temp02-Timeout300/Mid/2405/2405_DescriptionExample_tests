```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void testPartitionString() {
        Solution solution = new Solution();
        String s = "abacaba";
        int expectedOutput = 4;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void testPartitionString2() {
        Solution solution = new Solution();
        String s = "ssssss";
        int expectedOutput = 6;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void testPartitionString3() {
        Solution solution = new Solution();
        String s = "abcde";
        int expectedOutput = 5;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void testPartitionString4() {
        Solution solution = new Solution();
        String s = "";
        int expectedOutput = 0;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void testPartitionString5() {
        Solution solution = new Solution();
        String s = "a";
        int expectedOutput = 1;
        assertEquals(expectedOutput, solution.partition(s));
    }
}
