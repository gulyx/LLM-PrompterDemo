```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class SubrectangleQueries {

    private int[][] rectangle;

    public SubrectangleQueries(int[][] rectangle) {
        this.rectangle = rectangle;
    }

    @Test
    [--Example 1--]
    public void testGetValue() {
        updateSubrectangle(0, 2, 3, 4, 5);
        assertEquals(5, getValue(0, 2));
    }

    @Test
    [--Example 1--]
    public void testUpdateSubrectangle() {
        updateSubrectangle(0, 2, 3, 4, 10);
        assertEquals(10, getValue(3, 2));
    }

    @Test
    [--Example 2--]
    public void testGetValue_2() {
        updateSubrectangle(0, 0, 1, 1, 100);
        assertEquals(100, getValue(0, 0));
    }

    @Test
    [--Example 2--]
    public void testUpdateSubrectangle_2() {
        updateSubrectangle(0, 0, 1, 1, 20);
        assertEquals(20, getValue(2, 2));
    }
}
```