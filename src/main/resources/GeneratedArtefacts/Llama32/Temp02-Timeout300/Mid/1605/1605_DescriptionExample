```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertArrayEquals;

public class DescriptionExampleTest {

    @Test
    public void test_1() {
        int[][] rowSum = {{3, 8}};
        int[][] colSum = {{4, 7}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{3, 0}, {1, 7}}, result);
    }

    @Test
    public void test_2() {
        int[][] rowSum = {{5, 7, 10}};
        int[][] colSum = {{8, 6, 8}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{0, 5, 0}, {6, 1, 0}, {2, 0, 8}}, result);
    }

    @Test
    public void test_3() {
        int[][] rowSum = {{9}};
        int[][] colSum = {{10}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{9, 0}, {0, 1}}, result);
    }

    @Test
    public void test_4() {
        int[][] rowSum = {{3, 8}};
        int[][] colSum = {{4, 7, 8}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{3, 0}, {1, 7}}, result);
    }

    @Test
    public void test_5() {
        int[][] rowSum = {{10}};
        int[][] colSum = {{9}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{0, 1}, {2, 8}}, result);
    }
}

class Solution {

    public int[][] restoreMatrix(int[] rowSum, int[] colSum) {
        // implementation
    }
}
