package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1605_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[][] rowSum = {{9}, {3}};
        int[][] colSum = {{12}, {6}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test2() {
        int[][] rowSum = {{1, 3}, {4}};
        int[][] colSum = {{5}, {6}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test3() {
        int[][] rowSum = {{1, 1}, {1, 1}};
        int[][] colSum = {{2}, {2}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test4() {
        int[][] rowSum = {{1, 2}, {3}};
        int[][] colSum = {{5}, {7}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(2, result[0].length);
    }

    @Test
    public void test5() {
        int[][] rowSum = {{1, 2, 3}, {4}};
        int[][] colSum = {{6}, {7}};
        int[][] result = new Solution().restoreMatrix(rowSum, colSum);
        assertEquals(2, result.length);
        assertEquals(3, result[0].length);
    }
}