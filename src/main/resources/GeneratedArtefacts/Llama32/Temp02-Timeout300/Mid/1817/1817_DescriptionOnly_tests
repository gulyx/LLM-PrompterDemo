package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1817;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1817.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class SolutionTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[][] logs = {{2,0},{2,1},{1,0},{1,2}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assertEquals(3, answer[0]);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[][] logs = {{1,0},{1,1},{1,2},{2,0},{2,1},{2,2}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assertEquals(3, answer[0]);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[][] logs = {{1,0},{1,1},{2,0},{2,1}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assertEquals(4, answer[0]);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[][] logs = {{1,0},{2,0}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assertEquals(3, answer[0]);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[][] logs = {{1,0},{2,0}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assertEquals(3, answer[0]);
    }
}