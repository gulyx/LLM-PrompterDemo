package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2442;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2442.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void testReverseDigits() {
        Solution solution = new Solution();
        int[] nums = {1,13,10,12,31};
        assertEquals(6, solution.numDistinct(nums));
    }

    @Test
    public void testReverseDigits2() {
        Solution solution = new Solution();
        int[] nums = {2,2,2};
        assertEquals(1, solution.numDistinct(nums));
    }

    @Test
    public void testReverseDigits3() {
        Solution solution = new Solution();
        int[] nums = {10,12,13,21,31};
        assertEquals(5, solution.numDistinct(nums));
    }

    @Test
    public void testReverseDigits4() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5};
        assertEquals(11, solution.numDistinct(nums));
    }

    @Test
    public void testReverseDigits5() {
        Solution solution = new Solution();
        int[] nums = {};
        assertEquals(0, solution.numDistinct(nums));
    }
}
