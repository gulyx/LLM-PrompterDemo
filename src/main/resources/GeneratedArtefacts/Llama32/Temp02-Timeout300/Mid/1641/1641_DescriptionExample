package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTest {

    @Test
    public void test1() {
        int result = numVowelStrings(1);
        assertEquals(5, result);
    }

    @Test
    public void test2() {
        int result = numVowelStrings(2);
        assertEquals(15, result);
    }

    @Test
    public void test3() {
        int result = numVowelStrings(33);
        assertEquals(66045, result);
    }

    @Test
    public void test4() {
        int result = numVowelStrings(50);
        assertEquals(31287520, result);
    }

    @Test
    public void test5() {
        int result = numVowelStrings(1);
        assertEquals(5, result);
    }
}