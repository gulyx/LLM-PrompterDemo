package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2317;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2317.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {3, 2, 4, 6};
        int expected = 7;
        assertEquals(expected, solution.maxXOR(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 3, 9, 2};
        int expected = 11;
        assertEquals(expected, solution.maxXOR(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {5, 1, 4, 7, 8};
        int expected = 15;
        assertEquals(expected, solution.maxXOR(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {0, 2, 3, 4, 5};
        int expected = 5;
        assertEquals(expected, solution.maxXOR(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1, 1, 1, 1, 1};
        int expected = 0;
        assertEquals(expected, solution.maxXOR(nums));
    }
}
