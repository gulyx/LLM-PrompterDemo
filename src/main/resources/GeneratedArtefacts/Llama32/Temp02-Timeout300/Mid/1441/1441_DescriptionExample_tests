package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441.Solution;

public class DescriptionExampleTestsTest {

    public void testExample1() {
        Solution solution = new Solution();
        int[] target = {1, 3};
        int n = 3;
        String[] expected = {"Push", "Push", "Pop", "Push"};
        assertArrayEquals(expected, solution.buildArray(target, n));
    }

    public void testExample2() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3};
        int n = 3;
        String[] expected = {"Push", "Push", "Push"};
        assertArrayEquals(expected, solution.buildArray(target, n));
    }

    public void testExample3() {
        Solution solution = new Solution();
        int[] target = {1, 2};
        int n = 4;
        String[] expected = {"Push", "Push"};
        assertArrayEquals(expected, solution.buildArray(target, n));
    }

    public void testExample4() {
        Solution solution = new Solution();
        int[] target = {1, 3, 5};
        int n = 6;
        String[] expected = {"Push","Push","Pop","Push"};
        assertArrayEquals(expected, solution.buildArray(target, n));
    }

    public void testExample5() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3, 4};
        int n = 6;
        String[] expected = {"Push", "Push", "Pop", "Push"};
        assertArrayEquals(expected, solution.buildArray(target, n));
    }
}