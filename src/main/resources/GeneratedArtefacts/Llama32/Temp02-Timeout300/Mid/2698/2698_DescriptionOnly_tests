package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2698;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2698.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 2698_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int n = 13;
        int expected = 52;
        assertEquals(expected, solution.punishmentNumber(n));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int n = 19;
        int expected = 262;
        assertEquals(expected, solution.punishmentNumber(n));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int n = 1;
        int expected = 1;
        assertEquals(expected, solution.punishmentNumber(n));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int n = 1000;
        int expected = 25164150;
        assertEquals(expected, solution.punishmentNumber(n));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int n = 2;
        int expected = 5;
        assertEquals(expected, solution.punishmentNumber(n));
    }
}