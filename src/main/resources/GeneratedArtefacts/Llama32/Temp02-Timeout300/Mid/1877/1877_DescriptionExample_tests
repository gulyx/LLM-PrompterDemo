package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1877;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1877.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1877_DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {3,5,2,3};
        int expected = 7;
        assertEquals(expected, solution.maxPairSum(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {3,5,4,2,4,6};
        int expected = 8;
        assertEquals(expected, solution.maxPairSum(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1, 7, 11, 5};
        int expected = 9;
        assertEquals(expected, solution.maxPairSum(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 3, 4};
        int expected = 6;
        assertEquals(expected, solution.maxPairSum(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1, 1, 1, 1};
        int expected = 2;
        assertEquals(expected, solution.maxPairSum(nums));
    }
}