package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1561;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1561.Solution;

public class 1561_DescriptionOnly_testsTest {

    @Test
    public void test_0() {
        Solution solution = new Solution();
        int[] piles = {2,7,12};
        int expected = 18;
        int actual = solution.maxCoins(piles);
        assertEquals(expected, actual);
    }

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] piles = {1,2,3,4,5,6};
        int expected = 15;
        int actual = solution.maxCoins(piles);
        assertEquals(expected, actual);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] piles = {1,1,1,1,1,1};
        int expected = 3;
        int actual = solution.maxCoins(piles);
        assertEquals(expected, actual);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] piles = {2,7,12,1,1,6};
        int expected = 21;
        int actual = solution.maxCoins(piles);
        assertEquals(expected, actual);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] piles = {5,6,3,2,1};
        int expected = 9;
        int actual = solution.maxCoins(piles);
        assertEquals(expected, actual);
    }
}