```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1689;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1689.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        String n = "32";
        int expectedOutput = 3;
        assertEquals(expectedOutput, solution.minPartitions(n));
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        String n = "82734";
        int expectedOutput = 8;
        assertEquals(expectedOutput, solution.minPartitions(n));
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        String n = "27346209830709182346";
        int expectedOutput = 9;
        assertEquals(expectedOutput, solution.minPartitions(n));
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        String n = "1";
        int expectedOutput = 1;
        assertEquals(expectedOutput, solution.minPartitions(n));
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        String n = "0";
        int expectedOutput = 0;
        assertEquals(expectedOutput, solution.minPartitions(n));
    }
}
