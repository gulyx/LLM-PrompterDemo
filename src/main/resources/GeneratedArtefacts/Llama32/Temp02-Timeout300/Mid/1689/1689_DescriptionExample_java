```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1689;

public class Solution {
    public int minPartitions(String n) {
        int max = 0;
        for (char c : n.toCharArray()) {
            max = Math.max(max, c - '0');
        }
        return max;
    }

    [--Example 1--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.minPartitions("32")); // Output: 3
    }

    [--Example 2--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.minPartitions("82734")); // Output: 8
    }

    [--Example 3--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.minPartitions("27346209830709182346")); // Output: 9
    }
}
```