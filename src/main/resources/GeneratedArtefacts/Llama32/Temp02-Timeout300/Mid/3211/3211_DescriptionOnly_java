package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3211;

public class Solution {
    public List<String> validBinaryStrings(int n) {
        List<String> result = new ArrayList<>();
        for (int i = 0; i < n; i++) {
            if ((i & 1) == 0) {
                result.add("01");
            } else {
                result.add("10");
            }
        }
        return result;
    }
}