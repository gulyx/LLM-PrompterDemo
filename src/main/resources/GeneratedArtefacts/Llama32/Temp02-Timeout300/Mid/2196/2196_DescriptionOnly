package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 2196_DescriptionOnly {

    @Test
    public void test1() {
        int[][] descriptions = {{0,1,1},{0,2,0}};
        TreeNode root = new Solution().constructTree(descriptions);
        assertNotNull(root);
    }

    @Test
    public void test2() {
        int[][] descriptions = {{0,1,1},{0,2,0},{1,3,0},{2,4,1},{3,5,1}};
        TreeNode root = new Solution().constructTree(descriptions);
        assertNotNull(root);
    }

    @Test
    public void test3() {
        int[][] descriptions = {{0,1,1},{0,2,0},{1,3,0},{2,4,1},{3,5,1},{4,6,1}};
        TreeNode root = new Solution().constructTree(descriptions);
        assertNotNull(root);
    }

    @Test
    public void test4() {
        int[][] descriptions = {{0,1,1},{0,2,0},{1,3,0},{2,4,1},{3,5,1},{4,6,1},{5,7,1}};
        TreeNode root = new Solution().constructTree(descriptions);
        assertNotNull(root);
    }

    @Test
    public void test5() {
        int[][] descriptions = {{0,1,1},{0,2,0},{1,3,0},{2,4,1},{3,5,1},{4,6,1},{5,7,1},{6,8,1}};
        TreeNode root = new Solution().constructTree(descriptions);
        assertNotNull(root);
    }
}