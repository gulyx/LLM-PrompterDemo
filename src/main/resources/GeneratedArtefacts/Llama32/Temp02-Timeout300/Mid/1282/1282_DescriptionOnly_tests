package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1282;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1282.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1282_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] groupSizes = {3,3,3,3,3};
        Object result = solution.groupThePeople(groupSizes);
        assertNotNull(result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] groupSizes = {1,1,1,1,1};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(5, ((java.util.List<java.util.List<Integer>>)result).size());
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] groupSizes = {2,7,1,6,1};
        Object result = solution.groupThePeople(groupSizes);
        assertNotNull(result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] groupSizes = {2,2,3,3,3,4,4,5,5,5,6,7};
        Object result = solution.groupThePeople(groupSizes);
        assertNotNull(result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] groupSizes = {1,2,3,4,5};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(5, ((java.util.List<java.util.List<Integer>>)result).size());
    }
}