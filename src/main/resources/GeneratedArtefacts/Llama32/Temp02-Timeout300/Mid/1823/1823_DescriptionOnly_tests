package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1823;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1823.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 1823_DescriptionOnly_testsTest {

    @Test
    public void testExample1() {
        Solution solution = new Solution();
        int n = 5;
        int k = 2;
        assertEquals(4, solution.findWinner(n, k));
    }

    @Test
    public void testExample2() {
        Solution solution = new Solution();
        int n = 3;
        int k = 1;
        assertEquals(1, solution.findWinner(n, k));
    }

    @Test
    public void testExample3() {
        Solution solution = new Solution();
        int n = 500;
        int k = 250;
        assertEquals(1, solution.findWinner(n, k));
    }

    @Test
    public void testEdgeCase1() {
        Solution solution = new Solution();
        int n = 2;
        int k = 1;
        assertEquals(1, solution.findWinner(n, k));
    }

    @Test
    public void testEdgeCase2() {
        Solution solution = new Solution();
        int n = 500;
        int k = 499;
        assertEquals(1, solution.findWinner(n, k));
    }
}