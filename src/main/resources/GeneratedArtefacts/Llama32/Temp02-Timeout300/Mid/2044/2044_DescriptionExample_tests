```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {3, 1};
        int expectedOutput = 2;
        assertEquals(expectedOutput, solution.maxOr(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {2, 2, 2};
        int expectedOutput = 7;
        assertEquals(expectedOutput, solution.maxOr(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {3, 2, 1, 5};
        int expectedOutput = 6;
        assertEquals(expectedOutput, solution.maxOr(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {0, 0, 0};
        int expectedOutput = 1;
        assertEquals(expectedOutput, solution.maxOr(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 3, 4, 5};
        int expectedOutput = 15;
        assertEquals(expectedOutput, solution.maxOr(nums));
    }
}
```