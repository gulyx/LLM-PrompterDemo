```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.797;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.797.Solution;
import org.junit.Test;
import java.util.Arrays;
import java.util.List;

public class SolutionTest {

    @Test
    public void testAllPaths() {
        Solution solution = new Solution();
        List<List<Integer>> result = solution.allPathsSourceTarget(graph1);
        System.out.println(result);
    }

    @Test
    public void testAllPaths2() {
        Solution solution = new Solution();
        List<List<Integer>> result = solution.allPathsSourceTarget(graph2);
        System.out.println(result);
    }

    @Test
    public void testAllPaths3() {
        Solution solution = new Solution();
        List<List<Integer>> result = solution.allPathsSourceTarget(graph3);
        System.out.println(result);
    }

    @Test
    public void testAllPaths4() {
        Solution solution = new Solution();
        List<List<Integer>> result = solution.allPathsSourceTarget(graph4);
        System.out.println(result);
    }

    @Test
    public void testAllPaths5() {
        Solution solution = new Solution();
        List<List<Integer>> result = solution.allPathsSourceTarget(graph5);
        System.out.println(result);
    }

    private static final List<List<Integer>> graph1 = Arrays.asList(
            Arrays.asList(1, 2),
            Arrays.asList(3),
            Arrays.asList(3),
            Arrays.asList()
    );

    private static final List<List<Integer>> graph2 = Arrays.asList(
            Arrays.asList(4, 3, 1),
            Arrays.asList(3, 2, 4),
            Arrays.asList(3),
            Arrays.asList(4),
            Arrays.asList()
    );

    private static final List<List<Integer>> graph3 = Arrays.asList(
            Arrays.asList(),
            Arrays.asList(1),
            Arrays.asList(2)
    );

    private static final List<List<Integer>> graph4 = Arrays.asList(
            Arrays.asList(1, 2),
            Arrays.asList(2),
            Arrays.asList()
    );

    private static final List<List<Integer>> graph5 = Arrays.asList(
            Arrays.asList(),
            Arrays.asList(1, 2),
            Arrays.asList(2)
    );
}
```