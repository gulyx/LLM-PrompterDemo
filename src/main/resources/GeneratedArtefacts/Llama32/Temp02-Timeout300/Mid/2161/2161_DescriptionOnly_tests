package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2161;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2161.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class 2161_DescriptionOnly_testsTest {

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[] nums = {4,3,2,10,9,8};
        int pivot = 5;
        int[] result = solution.reorderArray(nums,pivot);
        assertArrayEquals(new int[]{2,1,20,3,11,4},result);
    }

    @Test
    public void test_Solution_2() {
        Solution solution = new Solution();
        int[] nums = {4,3,2,10,9,8};
        int pivot = 6;
        int[] result = solution.reorderArray(nums,pivot);
        assertArrayEquals(new int[]{2,1,20,3,11,4},result);
    }

    @Test
