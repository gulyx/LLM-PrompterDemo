package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2375;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2375.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class DescriptionOnly_testsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        String pattern = "ID";
        String num = solution.descriptionOnly(pattern);
        assertEquals("12", num);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        String pattern = "DD";
        String num = solution.descriptionOnly(pattern);
        assertEquals("98", num);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        String pattern = "IID";
        String num = solution.descriptionOnly(pattern);
        assertEquals("123", num);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        String pattern = "IDD";
        String num = solution.descriptionOnly(pattern);
        assertEquals("987", num);
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        String pattern = "IIIDDD";
        String num = solution.descriptionOnly(pattern);
        assertEquals("123456789", num);
    }
}