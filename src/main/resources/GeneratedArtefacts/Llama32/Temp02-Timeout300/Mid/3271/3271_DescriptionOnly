package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 3271_DescriptionOnly {

    @Test
    public void test_1() {
        String s = "abcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = hashString(s, k);
        assertEquals("abcdefghijklmnopqrstuvwxyz", result);
    }

    @Test
    public void test_2() {
        String s = "aabbccdd";
        int k = 4;
        String result = hashString(s, k);
        assertEquals("abcd", result);
    }

    @Test
    public void test_3() {
        String s = "abcdefghi";
        int k = 8;
        String result = hashString(s, k);
        assertEquals("abcdefghi", result);
    }

    @Test
    public void test_4() {
        String s = "abcdefghijklmnopqrstuvwxyz0123456789!@#$%^&*()_-+={}[]:;<>,.?/~`|";
        int k = 26;
        String result = hashString(s, k);
        assertEquals("abcdefghijklmnopqrstuvwxyz", result);
    }

    @Test
    public void test_5() {
        String s = "aabbccdd1234567890!@#$%^&*()_-+={}[]:;<>,.?/~`|";
        int k = 4;
        String result = hashString(s, k);
        assertEquals("abcd", result);
    }

}