package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 3271_DescriptionOnlyTest {

    @Test
    public void test_1() {
        String s = "abcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = solution(s, k);
        assertEquals("a", result);
    }

    @Test
    public void test_2() {
        String s = "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = solution(s, k);
        assertEquals("ab", result);
    }

    @Test
    public void test_3() {
        String s = "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = solution(s, k);
        assertEquals("abc", result);
    }

    @Test
    public void test_4() {
        String s = "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = solution(s, k);
        assertEquals("abcd", result);
    }

    @Test
    public void test_5() {
        String s = "abcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyzabcdefghijklmnopqrstuvwxyz";
        int k = 26;
        String result = solution(s, k);
        assertEquals("abcde", result);
    }
}