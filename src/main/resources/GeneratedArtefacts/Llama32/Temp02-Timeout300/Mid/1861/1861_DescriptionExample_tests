package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1861;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1861.Solution;

public class DescriptionExampleTestsTest {

    public void test_1() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#","."},{"."","#"}};
        char[][] expected = {{".","#"},{" ".","#"}};}
        assertArrayEquals(expected, solution.rotateTheBox(boxGrid));
    }

    public void test_2() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*","."},{"#","#","*","."}};
        char[][] expected = {{"#","."},{"#","#"},{"*","*"},{" ".","."}};}
        assertArrayEquals(expected, solution.rotateTheBox(boxGrid));
    }

    public void test_3() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#","#","*",".","*","."},{"#","#","#","*",".","."},{"#","#","#",".","#","."}};
        char[][] expected = {{".","#","#"},{" ".","#","#"},{" ","*","."},{"#","*","."},{"#",".","*"},{"#",".","."}};}
        assertArrayEquals(expected, solution.rotateTheBox(boxGrid));
    }

    public void test_4() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","#"},{" ".","."},{"#","*"}};
        char[][] expected = {{" ",".","."},{" ".","."},{"#","*"}};}
        assertArrayEquals(expected, solution.rotateTheBox(boxGrid));
    }

    public void test_5() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{" ".","*"},{"#","."}};
        char[][] expected = {{" ",".","*"},{" ".","*"},{"#","."}};}
        assertArrayEquals(expected, solution.rotateTheBox(boxGrid));
    }
}