public class 1551_DescriptionOnly_testsTest {

    public void test_1() {
        Solution solution = new Solution();
        int n = 3;
        int expected = 2;
        int actual = solution.makeArrayEqual(n);
        assert actual == expected : "Test 1 failed";
    }

    public void test_2() {
        Solution solution = new Solution();
        int n = 4;
