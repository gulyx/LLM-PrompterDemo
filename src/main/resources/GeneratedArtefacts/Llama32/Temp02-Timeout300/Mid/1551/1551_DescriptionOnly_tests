    }

    public void test_3() {
        Solution solution = new Solution();
        int n = 5;
        int expected = 8;
        int actual = solution.makeArrayEqual(n);
        assert actual == expected : "Test 3 failed";
    }

    public void test_4() {
        Solution solution = new Solution();
        int n = 1;
        int expected = 0;
        int actual = solution.makeArrayEqual(n);
        assert actual == expected : "Test 4 failed";
    }

    public void test_5() {
        Solution solution = new Solution();
        int n = 2;
