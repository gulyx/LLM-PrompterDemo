```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 2657_DescriptionOnly {

    @Test
    public void testPrefixCommonArray() {
        int[] A = {1,2,3};
        int[] B = {3,2,1};
        int[] expected = {1,2,3};
        int[] result = new int[A.length];
        for (int i = 0; i < A.length; i++) {
            for (int j : new int[]{A[i],B[i]}) {
                result[i]++;
            }
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void testPrefixCommonArray_2() {
        int[] A = {1, 3, 5};
        int[] B = {1, 2, 3};
        int[] expected = {1, 2, 3};
        int[] result = new int[A.length];
        for (int i = 0; i < A.length; i++) {
            for (int j : new int[]{A[i],B[i]}) {
                result[i]++;
            }
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void testPrefixCommonArray_3() {
        int[] A = {1, 2, 3};
        int[] B = {3, 2, 1};
        int[] expected = {1, 2, 3};
        int[] result = new int[A.length];
        for (int i = 0; i < A.length; i++) {
            for (int j : new int[]{A[i],B[i]}) {
                result[i]++;
            }
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void testPrefixCommonArray_4() {
        int[] A = {1, 2, 3, 4};
        int[] B = {4, 3, 2, 1};
        int[] expected = {1, 2, 3, 4};
        int[] result = new int[A.length];
        for (int i = 0; i < A.length; i++) {
            for (int j : new int[]{A[i],B[i]}) {
                result[i]++;
            }
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void testPrefixCommonArray_5() {
        int[] A = {1, 2, 3};
        int[] B = {1, 2, 3};
        int[] expected = {1, 2, 3};
        int[] result = new int[A.length];
        for (int i = 0; i < A.length; i++) {
            for (int j : new int[]{A[i],B[i]}) {
                result[i]++;
            }
        }
        assertArrayEquals(expected, result);
    }

}
