package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class 2657_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] A = {2,5,6};
        int[] B = {1,2,3};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{2, 2},result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] A = {1,2,3};
        int[] B = {4,5,6};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{0, 0},result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] A = {1,2,3,4,5,6,7};
        int[] B = {1,2,3,4,5,6,8};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{0, 0, 0, 0, 0, 0},result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] A = {1,2,3};
        int[] B = {1,2,3,4,5,6,7};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{0, 0, 0},result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] A = {1,2,3,4,5,6,7};
        int[] B = {1,2,3,4,5,6,7};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{0, 0, 0, 0, 0, 0, 0},result);
    }
}