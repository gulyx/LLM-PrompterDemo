package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class 2657_DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] A = {1,3,2,4};
        int[] B = {3,1,2,4};
        int[] result = solution.prefixCommonArray(A,B);
        assertArrayEquals(new int[]{0,2,3,4},result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] A = {2,3,1};
        int[] B = {3,1,2};
        int[] result = solution.prefixCommonArray(A,B);
        assertArrayEquals(new int[]{0,1,3},result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] A = {1,1,2,2};
        int[] B = {1,2,2,1};
        int[] result = solution.prefixCommonArray(A,B);
        assertArrayEquals(new int[]{0,1,2,3},result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] A = {1,2,3,4};
        int[] B = {4,3,2,1};
        int[] result = solution.prefixCommonArray(A,B);
        assertArrayEquals(new int[]{0,1,2,3},result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] A = {1,1,1,1};
        int[] B = {1,1,1,1};
        int[] result = solution.prefixCommonArray(A,B);
        assertArrayEquals(new int[]{0,1,2,3},result);
    }
}