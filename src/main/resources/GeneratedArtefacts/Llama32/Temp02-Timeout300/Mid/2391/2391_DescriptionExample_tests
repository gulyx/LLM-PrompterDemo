package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2391_DescriptionExampleTest {

    @Test
    public void test_1() {
        String[] garbage = {"G", "P", "GP", "GG"};
        int[] travel = {2, 4, 3};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_2() {
        String[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        assertEquals(37, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_3() {
        String[] garbage = {"G", "P", "MP", "GG"};
        int[] travel = {2, 4, 1, 3};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_4() {
        String[] garbage = {"M","PP","GPG"};
        int[] travel = {1,10,3};
        assertEquals(19, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_5() {
        String[] garbage = {"GG", "MP", "P"};
        int[] travel = {2, 4, 1};
        assertEquals(15, minGarbagePickup(garbage, travel));
    }
}