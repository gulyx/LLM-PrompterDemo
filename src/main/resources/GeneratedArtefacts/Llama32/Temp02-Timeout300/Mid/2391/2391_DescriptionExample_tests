package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2391;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2391.Solution;

public class DescriptionExampleTestsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] garbage = {"G","P","GP","GG"};
        int[] travel = {2,4,3};
        int expectedOutput = 21;
        assertEquals(expectedOutput, solution.minCost(garbage, travel));
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        int expectedOutput = 37;
        assertEquals(expectedOutput, solution.minCost(garbage, travel));
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] garbage = {"G","P","M"};
        int[] travel = {2,4,1};
        int expectedOutput = 9;
        assertEquals(expectedOutput, solution.minCost(garbage, travel));
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] garbage = {"P","G","M"};
        int[] travel = {2,3,1};
        int expectedOutput = 9;
        assertEquals(expectedOutput, solution.minCost(garbage, travel));
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[] garbage = {"G","P","GP","GG"};
        int[] travel = {2,4,3,1};
        int expectedOutput = 21;
        assertEquals(expectedOutput, solution.minCost(garbage, travel));
    }
}