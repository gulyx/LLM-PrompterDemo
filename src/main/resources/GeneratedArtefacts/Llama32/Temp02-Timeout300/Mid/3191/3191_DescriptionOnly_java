package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3191;

public class Solution {
    public int minFlips(int[] nums) {
        int count = 0;
        for (int num : nums) {
            if (num == 0) {
                count++;
            }
        }
        return Math.min(count, nums.length - count);
    }
}