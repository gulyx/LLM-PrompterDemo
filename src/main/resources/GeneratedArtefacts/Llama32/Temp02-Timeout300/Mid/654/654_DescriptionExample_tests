package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.654;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.654.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 654_DescriptionExample_testsTest {

    @Test
    public void testBuildTree() {
        Solution solution = new Solution();
        int[] nums = {3,2,1,6,0,5};
        TreeNode root = solution.buildMaxTree(nums);
        assertNotNull(root);
    }

    @Test
    public void testBuildTree_2() {
        Solution solution = new Solution();
        int[] nums = {3,2,1};
        TreeNode root = solution.buildMaxTree(nums);
        assertNotNull(root);
    }

    @Test
    public void testBuildTree_3() {
        Solution solution = new Solution();
        int[] nums = {6,5,4,3,2,1};
        TreeNode root = solution.buildMaxTree(nums);
        assertNotNull(root);
    }

    @Test
    public void testBuildTree_4() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5};
        TreeNode root = solution.buildMaxTree(nums);
        assertNotNull(root);
    }

    @Test
    public void testBuildTree_5() {
        Solution solution = new Solution();
        int[] nums = {};
        TreeNode root = solution.buildMaxTree(nums);
        assertNull(root);
    }
}