package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import java.util.*;

public class 3324_DescriptionOnlyTest {

    @Test
    public void test1() {
        String target = "leetcode";
        List<String> result = minSteps(target);
        assertEquals(Arrays.asList("leet", "code"), result);
    }

    @Test
    public void test2() {
        String target = "intention";
        List<String> result = minSteps(target);
        assertEquals(Arrays.asList("inten", "ion"), result);
    }

    @Test
    public void test3() {
        String target = "applepenapple";
        List<String> result = minSteps(target);
        assertEquals(Arrays.asList("applepenapple"), result);
    }

    @Test
    public void test4() {
        String target = "aabbcc";
        List<String> result = minSteps(target);
        assertEquals(Arrays.asList("aabbcc"), result);
    }

    @Test
    public void test5() {
        String target = "abcdefghijklmnopqrstuvwxyz";
        List<String> result = minSteps(target);
        assertEquals(Arrays.asList("abcdefghijklmnopqrstuvwxyz"), result);
    }
}