package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 885_DescriptionExampleTest {

    @Test
    public void test_1() {
        int[][] result = new int[4][2];
        int[] expected = {0, 1, 2, 3};
        for (int i = 0; i < 4; i++) {
            result[i][0] = 0;
            result[i][1] = expected[i];
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_2() {
        int[][] result = new int[21][2];
        int[] expected = {1, 4, 2, 5, 2, 4, 3, 3, 3, 2, 1, 1, 0, 1, 4, 0, 4, 4, 4, 3, 4, 2};
        for (int i = 0; i < 21; i++) {
            result[i][0] = 0;
            result[i][1] = expected[i];
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_3() {
        int[][] result = new int[10][2];
        int[] expected = {1, 4, 2, 5, 2, 4, 3, 3, 3, 2};
        for (int i = 0; i < 10; i++) {
            result[i][0] = 0;
            result[i][1] = expected[i];
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_4() {
        int[][] result = new int[15][2];
        int[] expected = {1, 4, 2, 5, 2, 4, 3, 3, 3, 2, 1, 2, 1, 0, 4};
        for (int i = 0; i < 15; i++) {
            result[i][0] = 0;
            result[i][1] = expected[i];
        }
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_5() {
        int[][] result = new int[16][2];
        int[] expected = {1, 4, 2, 5, 2, 4, 3, 3, 3, 2, 1, 2, 1, 0, 4, 0};
        for (int i = 0; i < 16; i++) {
            result[i][0] = 0;
            result[i][1] = expected[i];
        }
        assertArrayEquals(expected, result);
    }

}