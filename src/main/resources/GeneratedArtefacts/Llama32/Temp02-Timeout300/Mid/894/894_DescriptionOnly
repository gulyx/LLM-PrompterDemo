package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
import java.util.*;

public class 894_DescriptionOnlyTest {

    @Test
    public void testGenerateTrees() {
        List<TreeNode> result = generateTrees(0);
        assertEquals(1, result.size());
    }

    @Test
    public void testGenerateTrees_WithOneNode() {
        List<TreeNode> result = generateTrees(1);
        assertEquals(2, result.size());
    }

    @Test
    public void testGenerateTrees_WithTwoNodes() {
        List<TreeNode> result = generateTrees(2);
        assertEquals(3, result.size());
    }

    @Test
    public void testGenerateTrees_WithThreeNodes() {
        List<TreeNode> result = generateTrees(3);
        assertEquals(5, result.size());
    }

    @Test
    public void testGenerateTrees_WithFourNodes() {
        List<TreeNode> result = generateTrees(4);
        assertEquals(7, result.size());
    }
}