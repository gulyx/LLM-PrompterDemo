package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 950_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] deck = {2,4,1};
        String result = "1124";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test2() {
        int[] deck = {5, 3, 8, 4, 2};
        String result = "83254";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test3() {
        int[] deck = {1, 100, 2, 12, 3};
        String result = "1321103";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test4() {
        int[] deck = {5, 4, 6, 2, 8, 10};
        String result = "2685102";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test5() {
        int[] deck = {1, 3, 5, 7, 9};
        String result = "13579";
        assertEquals(result, solve(deck).toString());
    }
}