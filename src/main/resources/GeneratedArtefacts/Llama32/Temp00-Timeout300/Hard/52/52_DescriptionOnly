import java.util.*;

public class SolutionTest {

    @Test
    public void testNQueens_1() {
        int n = 1;
        int expected = 1;
        Solution solution = new Solution();
        int actual = solution.totalNQueens(n);
        assertEquals(expected, actual);
    }

