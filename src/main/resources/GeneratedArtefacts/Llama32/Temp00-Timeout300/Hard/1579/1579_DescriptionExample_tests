package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1579_DescriptionExampleTest {

    @Test
    public void test_1() {
        int n = 4;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}};
        assertEquals(2, maxNumEdgesToRemove(n, edges));
    }

    @Test
    public void test_2() {
        int n = 4;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,4},{2,1,4}};
        assertEquals(0, maxNumEdgesToRemove(n, edges));
    }

    @Test
    public void test_3() {
        int n = 4;
        int[][] edges = {{3,2,3},{1,1,2},{2,3,4}};
        assertEquals(-1, maxNumEdgesToRemove(n, edges));
    }

    @Test
    public void test_4() {
        int n = 5;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}};
        assertEquals(0, maxNumEdgesToRemove(n, edges));
    }

    @Test
    public void test_5() {
        int n = 6;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}};
        assertEquals(0, maxNumEdgesToRemove(n, edges));
    }
}

