```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1579;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1579.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int n = 4;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}};
        assertEquals(2, solution.numEdgesToRemove(n, edges));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int n = 4;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,4},{2,1,4}};
        assertEquals(0, solution.numEdgesToRemove(n, edges));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int n = 4;
        int[][] edges = {{3,2,3},{1,1,2},{2,3,4}};
        assertEquals(-1, solution.numEdgesToRemove(n, edges));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int n = 5;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,3},{1,2,4},{1,1,2},{2,3,4}};
        assertEquals(0, solution.numEdgesToRemove(n, edges));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int n = 4;
        int[][] edges = {{3,1,2},{3,2,3},{1,1,4},{2,1,4}};
        assertEquals(0, solution.numEdgesToRemove(n, edges));
    }
}
