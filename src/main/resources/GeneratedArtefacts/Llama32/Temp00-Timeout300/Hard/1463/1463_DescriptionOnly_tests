package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1463;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1463.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertThrows;

public class 1463_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[][] grid = {{1,2,1},{1,5,1},{2,1,1}};
        assertEquals(9, solution.maxCherries(grid));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[][] grid = {{0}};
        assertEquals(0, solution.maxCherries(grid));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[][] grid = {{1, 1}, {1, 1}};
        assertEquals(2, solution.maxCherries(grid));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[][] grid = {{0, 0}, {0, 0}};
        assertEquals(0, solution.maxCherries(grid));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[][] grid = {{1,2},{3,4}};
        assertEquals(10, solution.maxCherries(grid));
    }
}