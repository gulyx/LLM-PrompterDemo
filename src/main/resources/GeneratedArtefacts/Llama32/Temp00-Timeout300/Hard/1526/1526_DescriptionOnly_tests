package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1526;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1526.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3};
        int[] initial = {0, 0, 0};
        assertEquals(3, solution.makeFingerPointingAtYou(target, initial));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] target = {1, 1, 1};
        int[] initial = {0, 0, 0};
        assertEquals(3, solution.makeFingerPointingAtYou(target, initial));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] target = {2, 2, 2};
        int[] initial = {0, 0, 0};
        assertEquals(6, solution.makeFingerPointingAtYou(target, initial));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] target = {1, 0, 0};
        int[] initial = {0, 0, 0};
        assertEquals(2, solution.makeFingerPointingAtYou(target, initial));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] target = {1, 1, 0};
        int[] initial = {0, 0, 0};
        assertEquals(3, solution.makeFingerPointingAtYou(target, initial));
    }
}