public class 1312_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String s = "ab";
        int expected = 1;
        int actual = solution.minInsertion(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String s = "a";
        int expected = 0;
        int actual = solution.minInsertion(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String s = "racecar";
        int expected = 0;
        int actual = solution.minInsertion(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String s = "abcba";
        int expected = 0;
        int actual = solution.minInsertion(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String s = "abccba";
        int expected = 0;
        int actual = solution.minInsertion(s);
        assertEquals(expected, actual);
    }
}