package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1402_DescriptionOnly {

    @Test
    public void test1() {
        int[] time = {1, 2};
        int[] satisfaction = {-1, -1};
        assertEquals(0, maxSatisfaction(time, satisfaction));
    }

    @Test
    public void test2() {
        int[] time = {1, 2, 3};
        int[] satisfaction = {1, 2, 3};
        assertEquals(9, maxSatisfaction(time, satisfaction));
    }

    @Test
    public void test3() {
        int[] time = {1, 2, 3};
        int[] satisfaction = {-1, -2, -3};
        assertEquals(-6, maxSatisfaction(time, satisfaction));
    }

    @Test
    public void test4() {
        int[] time = {1, 2, 3};
        int[] satisfaction = {0, 0, 0};
        assertEquals(0, maxSatisfaction(time, satisfaction));
    }

    @Test
    public void test5() {
        int[] time = {1, 2, 3};
        int[] satisfaction = {-1000, -1000, -1000};
        assertEquals(-3000, maxSatisfaction(time, satisfaction));
    }
}