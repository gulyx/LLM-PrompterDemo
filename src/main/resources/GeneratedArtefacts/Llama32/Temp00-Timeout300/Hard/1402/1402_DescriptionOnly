package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1402_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] satisfaction = {10, -5, -3, 5, 8};
        assertEquals(84, maxSatisfaction(satisfaction));
    }

    @Test
    public void test2() {
        int[] satisfaction = {-1000, -500, -300, -200, -100};
        assertEquals(-60000, maxSatisfaction(satisfaction));
    }

    @Test
    public void test3() {
        int[] satisfaction = {1, 2, 3, 4, 5};
        assertEquals(15, maxSatisfaction(satisfaction));
    }

    @Test
    public void test4() {
        int[] satisfaction = {-1000, -500, -300, -200, -100, 0, 1, 2, 3, 4, 5};
        assertEquals(25, maxSatisfaction(satisfaction));
    }

    @Test
    public void test5() {
        int[] satisfaction = {10, -5, -3, 5, 8, 1, 2, 3, 4, 5};
        assertEquals(84, maxSatisfaction(satisfaction));
    }
}