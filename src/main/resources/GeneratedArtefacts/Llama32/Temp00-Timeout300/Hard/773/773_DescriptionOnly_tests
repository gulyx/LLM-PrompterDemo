package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertThrows;

public class 773_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{{1,2,3},{4,5,0}});
        assertEquals(6, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{{1,2,3},{4,5,7}});
        assertEquals(-1, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{{0,1,2},{3,4,5}});
        assertEquals(6, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{{1,2,3},{4,5,0}, {7, 8,9}});
        assertEquals(-1, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{{1,2,3},{4,5,6}});
        assertEquals(0, result);
    }
}