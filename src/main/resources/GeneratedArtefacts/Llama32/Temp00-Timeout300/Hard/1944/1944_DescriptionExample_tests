package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1944_DescriptionExample_testsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] heights = {10,6,8,5,11,9};
        int[] answer = solution.couldSeePerson(heights);
        assertArrayEquals(new int[]{3,1,2,1,1,0}, answer);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] heights = {5,1,2,3,10};
        int[] answer = solution.couldSeePerson(heights);
        assertArrayEquals(new int[]{4,1,1,1,0}, answer);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] heights = {5,9,3,7};
        int[] answer = solution.couldSeePerson(heights);
        assertArrayEquals(new int[]{2,1,1,0}, answer);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] heights = {6,5,4,8};
        int[] answer = solution.couldSeePerson(heights);
        assertArrayEquals(new int[]{2,1,0,0}, answer);
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[] heights = {1,3,5,7};
        int[] answer = solution.couldSeePerson(heights);
        assertArrayEquals(new int[]{4,0,0,0}, answer);
    }
}
