        Solution solution = new Solution();
        int[][] rowConditions = {{1, 2}, {3, 2}};
        int[][] colConditions = {{2, 1}, {3, 2}};
        int[][] expected = {{3, 0, 0}, {0, 0, 1}, {0, 2, 0}};
        assertArrayEquals(expected, solution.solve(k, rowConditions, colConditions));
    }

