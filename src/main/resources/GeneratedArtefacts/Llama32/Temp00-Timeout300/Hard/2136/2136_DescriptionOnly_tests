package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2136;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2136.Solution;

public class 2136_DescriptionOnly_testsTest {

    public void test1() {
        Solution solution = new Solution();
        int[] plantTime = {3, 2};
        int[] growTime = {1, 2};
        int expected = 5;
        int actual = solution.solve(plantTime, growTime);
        assert actual == expected : "Test 1 failed";
    }

    public void test2() {
        Solution solution = new Solution();
        int[] plantTime = {1, 3, 2};
        int[] growTime = {2, 1, 4};
        int expected = 8;
        int actual = solution.solve(plantTime, growTime);
        assert actual == expected : "Test 2 failed";
    }

    public void test3() {
        Solution solution = new Solution();
        int[] plantTime = {2, 1, 1};
        int[] growTime = {4, 2, 1};
        int expected = 6;
        int actual = solution.solve(plantTime, growTime);
        assert actual == expected : "Test 3 failed";
    }

    public void test4() {
        Solution solution = new Solution();
        int[] plantTime = {1, 1, 1};
        int[] growTime = {2, 2, 2};
        int expected = 6;
        int actual = solution.solve(plantTime, growTime);
        assert actual == expected : "Test 4 failed";
    }

    public void test5() {
        Solution solution = new Solution();
        int[] plantTime = {1, 2, 3};
        int[] growTime = {1, 2, 3};
        int expected = 9;
        int actual = solution.solve(plantTime, growTime);
        assert actual == expected : "Test 5 failed";
    }
}