package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2130;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2130.Solution;

public class DescriptionExampleTestsTest {

    public void testMaxTwinSum() {
        Solution solution = new Solution();
        ListNode head1 = new ListNode(5);
        head1.next = new ListNode(4);
        head1.next.next = new ListNode(2);
        head1.next.next.next = new ListNode(1);
        System.out.println(solution.maxTwinSum(head1)); // 6
    }

    public void testMaxTwinSum_2() {
        Solution solution = new Solution();
        ListNode head2 = new ListNode(4);
        head2.next = new ListNode(2);
        head2.next.next = new ListNode(2);
        head2.next.next.next = new ListNode(3);
        System.out.println(solution.maxTwinSum(head2)); // 7
    }

    public void testMaxTwinSum_3() {
        Solution solution = new Solution();
        ListNode head3 = new ListNode(1);
        head3.next = new ListNode(4);
        head3.next.next = new ListNode(2);
        head3.next.next.next = new ListNode(5);
        System.out.println(solution.maxTwinSum(head3)); // 6
    }

    public void testMaxTwinSum_4() {
        Solution solution = new Solution();
        ListNode head4 = new ListNode(1);
        head4.next = new ListNode(2);
        head4.next.next = new ListNode(4);
        head4.next.next.next = new ListNode(5);
        System.out.println(solution.maxTwinSum(head4)); // 7
    }

    public void testMaxTwinSum_5() {
        Solution solution = new Solution();
        ListNode head5 = new ListNode(1);
        head5.next = new ListNode(2);
        head5.next.next = new ListNode(3);
        head5.next.next.next = new ListNode(4);
        System.out.println(solution.maxTwinSum(head5)); // 8
    }
}