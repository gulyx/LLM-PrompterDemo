package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertTrue;

public class 2405_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String s = "aab";
        int expected = 2;
        int actual = solution.partition(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String s = "abccde";
        int expected = 5;
        int actual = solution.partition(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String s = "abcde";
        int expected = 5;
        int actual = solution.partition(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String s = "aaa";
        int expected = 1;
        int actual = solution.partition(s);
        assertEquals(expected, actual);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String s = "";
        int expected = 0;
        int actual = solution.partition(s);
        assertEquals(expected, actual);
    }
}