package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2405.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String s = "abacaba";
        int expectedOutput = 4;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String s = "ssssss";
        int expectedOutput = 6;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String s = "abc";
        int expectedOutput = 3;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String s = "";
        int expectedOutput = 0;
        assertEquals(expectedOutput, solution.partition(s));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String s = "aaaa";
        int expectedOutput = 1;
        assertEquals(expectedOutput, solution.partition(s));
    }
}