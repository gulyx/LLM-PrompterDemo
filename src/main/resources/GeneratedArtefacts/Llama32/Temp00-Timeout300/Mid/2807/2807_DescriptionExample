```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExampleTest {

    @Test
    public void testInsertGCD() {
        ListNode head = new ListNode(18);
        head.next = new ListNode(6);
        head.next.next = new ListNode(10);
        head.next.next.next = new ListNode(3);

        ListNode result = insertGCD(head);
        while (result != null) {
            System.out.print(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDSingleNode() {
        ListNode head = new ListNode(7);

        ListNode result = insertGCD(head);
        while (result != null) {
            System.out.print(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDTwoNodes() {
        ListNode head = new ListNode(12);
        head.next = new ListNode(18);

        ListNode result = insertGCD(head);
        while (result != null) {
            System.out.print(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDThreeNodes() {
        ListNode head = new ListNode(15);
        head.next = new ListNode(20);
        head.next.next = new ListNode(30);

        ListNode result = insertGCD(head);
        while (result != null) {
            System.out.print(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDFourNodes() {
        ListNode head = new ListNode(8);
        head.next = new ListNode(12);
        head.next.next = new ListNode(24);
        head.next.next.next = new ListNode(18);

        ListNode result = insertGCD(head);
        while (result != null) {
            System.out.print(result.val + " ");
            result = result.next;
        }
    }

}

class ListNode {
    int val;
    ListNode next;

    ListNode(int x) { val = x; }
}
```