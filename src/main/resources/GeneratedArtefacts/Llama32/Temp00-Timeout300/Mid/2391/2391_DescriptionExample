package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class 2391_DescriptionExampleTest {

    @Test
    public void test_1() {
        String[] garbage = {"G","P","GP","GG"};
        int[] travel = {2,4,3};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_2() {
        String[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        assertEquals(37, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_3() {
        String[] garbage = {"G","P","MP","GG"};
        int[] travel = {2,4,3,1};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_4() {
        String[] garbage = {"M","MP","PG"};
        int[] travel = {3,10,5};
        assertEquals(27, minGarbagePickup(garbage, travel));
    }

    @Test
    public void test_5() {
        String[] garbage = {"G","P","GP","GG","MP"};
        int[] travel = {2,4,3,1,2};
        assertEquals(21, minGarbagePickup(garbage, travel));
    }
}