package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2391;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2391.Solution;

public class DescriptionExampleTestsTest {

    public void test_1() {
        Solution solution = new Solution();
        String[] garbage = {"G","P","GP","GG"};
        int[] travel = {2,4,3};
        long expectedOutput = 21;
        long actualOutput = solution.minGarbage(garbage, travel);
        assert actualOutput == expectedOutput : "Test case 1 failed";
    }

    public void test_2() {
        Solution solution = new Solution();
        String[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        long expectedOutput = 37;
        long actualOutput = solution.minGarbage(garbage, travel);
        assert actualOutput == expectedOutput : "Test case 2 failed";
    }

    public void test_3() {
        Solution solution = new Solution();
        String[] garbage = {"G","P"};
        int[] travel = {1,2};
        long expectedOutput = 5;
        long actualOutput = solution.minGarbage(garbage, travel);
        assert actualOutput == expectedOutput : "Test case 3 failed";
    }

    public void test_4() {
        Solution solution = new Solution();
        String[] garbage = {"M","P","G"};
        int[] travel = {1,2,3};
        long expectedOutput = 6;
        long actualOutput = solution.minGarbage(garbage, travel);
        assert actualOutput == expectedOutput : "Test case 4 failed";
    }

    public void test_5() {
        Solution solution = new Solution();
        String[] garbage = {"G","P","GP","GG"};
        int[] travel = {2,4,3,1};
        long expectedOutput = 21;
        long actualOutput = solution.minGarbage(garbage, travel);
        assert actualOutput == expectedOutput : "Test case 5 failed";
    }
}