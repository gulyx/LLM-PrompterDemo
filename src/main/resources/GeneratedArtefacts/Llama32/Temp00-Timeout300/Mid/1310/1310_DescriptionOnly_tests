package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] arr = { 1, 3 };
        int[][] queries = {{ 0, 1 }}; 
        int[] answer = new Solution().xorQueries(arr, queries);
        assertArrayEquals(new int[]{2}, answer);
    }

    @Test
    public void test2() {
        int[] arr = { 2, 4 };
        int[][] queries = {{ 0, 1 }}; 
        int[] answer = new Solution().xorQueries(arr, queries);
        assertArrayEquals(new int[]{6}, answer);
    }

    @Test
    public void test3() {
        int[] arr = { 5, 9, 3 };
        int[][] queries = {{ 0, 2 }}; 
        int[] answer = new Solution().xorQueries(arr, queries);
        assertArrayEquals(new int[]{8}, answer);
    }

    @Test
    public void test4() {
        int[] arr = { 1, 5, 10, 20, 28 };
        int[][] queries = {{ 0, 4 }}; 
        int[] answer = new Solution().xorQueries(arr, queries);
        assertArrayEquals(new int[]{31}, answer);
    }

    @Test
    public void test5() {
        int[] arr = { 1, 2, 3 };
        int[][] queries = {{ 0, 2 }}; 
        int[] answer = new Solution().xorQueries(arr, queries);
        assertArrayEquals(new int[]{4}, answer);
    }
}