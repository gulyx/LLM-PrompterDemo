package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1861;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1861.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1861_DescriptionOnly_testsTest {

    @Test
    public void test_0() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{".",".","."}};
        char[][] expected = {{".",".","*"},{"#",".","."}};
        char[][] result = solution.rotateTheBox(boxGrid);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_1() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{".",".","."},{"#",".","."}};
        char[][] expected = {{".",".","*"},{"#",".","."},{"#",".","."}};
        char[][] result = solution.rotateTheBox(boxGrid);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{".",".","."},{"#",".","."},{"#",".","."}};
        char[][] expected = {{".",".","*"},{"#",".","."},{"#",".","."},{"#",".","."}};
        char[][] result = solution.rotateTheBox(boxGrid);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{".",".","."},{"#",".","."},{"#",".","."},{"#",".","."}};
        char[][] expected = {{".",".","*"},{"#",".","."},{"#",".","."},{"#",".","."},{"#",".","."}};
        char[][] result = solution.rotateTheBox(boxGrid);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        char[][] boxGrid = {{"#",".","*"},{".",".","."},{"#",".","."},{"#",".","."},{"#",".","."},{"#",".","."}};
        char[][] expected = {{".",".","*"},{"#",".","."},{"#",".","."},{"#",".","."},{"#",".","."},{"#",".","."}};
        char[][] result = solution.rotateTheBox(boxGrid);
        assertArrayEquals(expected, result);
    }
}