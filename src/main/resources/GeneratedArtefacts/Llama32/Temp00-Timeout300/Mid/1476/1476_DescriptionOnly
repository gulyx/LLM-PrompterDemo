package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class SubrectangleQueries {

    @Test
    public void testUpdateSubrectangle() {
        int[][] rectangle = {{1, 2}, {3, 4}};
        SubrectangleQueries obj = new SubrectangleQueries(rectangle);
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 0));
    }

    @Test
    public void testUpdateSubrectangle_2() {
        int[][] rectangle = {{1, 2}, {3, 4}};
        SubrectangleQueries obj = new SubrectangleQueries(rectangle);
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(0, 1));
    }

    @Test
    public void testUpdateSubrectangle_3() {
        int[][] rectangle = {{1, 2}, {3, 4}};
        SubrectangleQueries obj = new SubrectangleQueries(rectangle);
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(1, 0));
    }

    @Test
    public void testUpdateSubrectangle_4() {
        int[][] rectangle = {{1, 2}, {3, 4}};
        SubrectangleQueries obj = new SubrectangleQueries(rectangle);
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertEquals(5, obj.getValue(1, 1));
    }

    @Test
    public void testUpdateSubrectangle_5() {
        int[][] rectangle = {{1, 2}, {3, 4}};
        SubrectangleQueries obj = new SubrectangleQueries(rectangle);
        obj.updateSubrectangle(0, 0, 1, 1, 5);
        assertNotEquals(1, obj.getValue(0, 0));
    }
}