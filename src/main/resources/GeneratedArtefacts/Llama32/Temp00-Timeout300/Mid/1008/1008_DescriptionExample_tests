package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExampleTest {

    @Test
    public void testConstructTreeFromPreorder() {
        Solution solution = new Solution();
        int[] preorder = {8,5,1,7,10,12};
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void testConstructTreeFromPreorder2() {
        Solution solution = new Solution();
        int[] preorder = {1,3};
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void testConstructTreeFromPreorder3() {
        Solution solution = new Solution();
        int[] preorder = {8,5,1,7,10,12,15};
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void testConstructTreeFromPreorder4() {
        Solution solution = new Solution();
        int[] preorder = {2,1,3};
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void testConstructTreeFromPreorder5() {
        Solution solution = new Solution();
        int[] preorder = {};
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNull(root);
    }
}

class Solution {

    public TreeNode bstFromPreorder(int[] preorder) {
        return null;
    }

}
