package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1441_DescriptionOnly {

    @Test
    public void test_1() {
        int[] target = {1,2,3};
        int n = 3;
        String result = "Push 1\nPush 2\nPush 3";
        assertEquals(result, solve(target, n));
    }

    @Test
    public void test_2() {
        int[] target = {1,1,1};
        int n = 3;
        String result = "Push 1\nPush 1\nPush 1";
        assertEquals(result, solve(target, n));
    }

    @Test
    public void test_3() {
        int[] target = {2,1,2};
        int n = 3;
        String result = "Push 1\nPush 2\nPop\nPush 2\nPush 1";
        assertEquals(result, solve(target, n));
    }

    @Test
    public void test_4() {
        int[] target = {5,1,5,5};
        int n = 5;
        String result = "Push 1\nPush 5\nPush 5\nPush 5";
        assertEquals(result, solve(target, n));
    }

    @Test
    public void test_5() {
        int[] target = {2,4,1};
        int n = 6;
        String result = "Push 1\nPush 2\nPush 4\nPop\nPush 2\nPush 1";
        assertEquals(result, solve(target, n));
    }
}