package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3};
        int n = 3;
        String result = solution.buildArray(target, n);
        assertEquals("[Push, Push, Pop, Push, Pop]", result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] target = {1, 1, 1};
        int n = 3;
        String result = solution.buildArray(target, n);
        assertEquals("[Push, Push, Pop, Push, Pop]", result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] target = {2, 4, 6};
        int n = 6;
        String result = solution.buildArray(target, n);
        assertEquals("[Push, Push, Pop, Push, Push, Pop]", result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3, 4};
        int n = 5;
        String result = solution.buildArray(target, n);
        assertEquals("[Push, Push, Pop, Push, Push, Pop]", result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] target = {1, 2, 3};
        int n = 4;
        String result = solution.buildArray(target, n);
        assertEquals("[Push, Push, Pop, Push, Pop]", result);
    }
}