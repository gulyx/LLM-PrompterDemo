package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.763;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.763.Solution;

public class DescriptionExampleTests {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] result = solution.descriptionExample("ababcbacadefegdehijhklij");
        assertEquals(9, result[0]);
        assertEquals(7, result[1]);
        assertEquals(8, result[2]);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] result = solution.descriptionExample("eccbbbbdec");
        assertEquals(10, result[0]);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] result = solution.descriptionExample("abcabcabc");
        assertEquals(9, result[0]);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] result = solution.descriptionExample("abababab");
        assertEquals(6, result[0]);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] result = solution.descriptionExample("");
        assertEquals(1, result[0]);
    }
}