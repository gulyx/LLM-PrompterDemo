```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3324;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3324.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 3324_DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String[] result = solution.findWords("abc");
        assertEquals(new String[]{"a","aa","ab","aba","abb","abc"}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String[] result = solution.findWords("he");
        assertEquals(new String[]{"a","b","c","d","e","f","g","h","ha","hb","hc","hd","he"}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String[] result = solution.findWords("");
        assertTrue(result.length == 0);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String[] result = solution.findWords("a");
        assertEquals(new String[]{"a"}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String[] result = solution.findWords("aa");
        assertEquals(new String[]{"aa"}, result);
    }
}
```