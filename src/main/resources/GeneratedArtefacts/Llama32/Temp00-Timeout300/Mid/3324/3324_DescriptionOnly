package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import java.util.*;

public class DescriptionOnly {

    public List<String> findDifferentKeys(String target) {
        List<String> result = new ArrayList<>();
        Deque<Character> queue = new ArrayDeque<>();
        Set<Character> visited = new HashSet<>();

        queue.add(target.charAt(0));
        result.add(String.valueOf(queue.peek()));

        for (int i = 1; i < target.length(); i++) {
            if (!queue.isEmpty()) {
                char lastChar = queue.poll();
                if (lastChar == target.charAt(i)) {
                    queue.add(lastChar);
                    result.add(String.valueOf(queue.peek()));
                } else {
                    queue.add(target.charAt(i));
                    result.add(String.valueOf(queue.peek()));
                }
            } else {
                queue.add(target.charAt(i));
                result.add(String.valueOf(queue.peek()));
            }

        }
        return result;
    }

}