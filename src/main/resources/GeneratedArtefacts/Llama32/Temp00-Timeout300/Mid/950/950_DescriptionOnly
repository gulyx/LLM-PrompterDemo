package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 950_DescriptionOnlyTest {

    @Test
    public void test_1() {
        int[] deck = {2,4,1};
        String result = "124";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test_2() {
        int[] deck = {5, 3, 8, 4, 2};
        String result = "83254";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test_3() {
        int[] deck = {1, 100, 2};
        String result = "21100";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test_4() {
        int[] deck = {5, 8, 9, 10, 1, 2, 3, 4, 6, 7};
        String result = "18976543";
        assertEquals(result, solve(deck).toString());
    }

    @Test
    public void test_5() {
        int[] deck = {1, 2, 3, 4, 5};
        String result = "12345";
        assertEquals(result, solve(deck).toString());
    }
}