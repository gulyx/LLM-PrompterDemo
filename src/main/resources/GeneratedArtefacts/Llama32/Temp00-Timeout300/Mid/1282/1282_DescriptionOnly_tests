package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1282;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1282.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1282_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] groupSizes = {3,3,3,3,3};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(3, ((int[])result)[0].length);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] groupSizes = {2,7,1,7,6,1,7};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(4, ((int[])result)[0].length);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] groupSizes = {2,2,3,3,3,4};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(5, ((int[])result)[0].length);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] groupSizes = {1,1,2,2,3,3,3};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(6, ((int[])result)[0].length);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] groupSizes = {1,1,1,1,1,2,2};
        Object result = solution.groupThePeople(groupSizes);
        assertEquals(4, ((int[])result)[0].length);
    }
}