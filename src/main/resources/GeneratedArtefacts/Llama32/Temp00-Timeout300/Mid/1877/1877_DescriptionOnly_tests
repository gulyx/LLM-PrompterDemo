package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1877;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1877.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 1877_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4};
        assertEquals(5, solution.maxPairSum(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {1,7,11,15};
        assertEquals(9, solution.maxPairSum(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5,6};
        assertEquals(7, solution.maxPairSum(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {100, 50, 200, 10};
        assertEquals(110, solution.maxPairSum(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5,6,7,8};
        assertEquals(9, solution.maxPairSum(nums));
    }
}