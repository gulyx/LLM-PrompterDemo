package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2317_DescriptionOnly {

    @Test
    public void test1() {
        int[] nums = {3, 4, 5};
        assertEquals(15, maxXOR(nums));
    }

    @Test
    public void test2() {
        int[] nums = {10, 20, 30};
        assertEquals(62, maxXOR(nums));
    }

    @Test
    public void test3() {
        int[] nums = {7, 12, 33, 5, 2};
        assertEquals(171, maxXOR(nums));
    }

    @Test
    public void test4() {
        int[] nums = {1, 2, 3, 4, 5};
        assertEquals(15, maxXOR(nums));
    }

    @Test
    public void test5() {
        int[] nums = {0, 1, 2, 3, 4};
        assertEquals(7, maxXOR(nums));
    }
}

public class Solution {

    public int maxXOR(int[] nums) {
        int n = nums.length;
        int[] res = new int[n];
        for (int i = 0; i < n; i++) {
            res[i] = nums[i];
        }

        int max = 0;
        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                max = Math.max(max, res[i] ^ res[j]);
            }
        }

        return max;
    }
}