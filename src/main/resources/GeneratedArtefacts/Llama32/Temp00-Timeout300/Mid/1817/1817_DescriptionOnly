package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import java.util.*;

public class DescriptionOnly {

    public int[] describingUserMinutes(int[][] logs, int k) {
        Map<Integer, Set<Integer>> minutes = new HashMap<>();
        for (int[] log : logs) {
            minutes.computeIfAbsent(log[0], key -> new HashSet<>()).add(log[1]);
        }
        List<Map.Entry<Integer, Integer>> list = new ArrayList<>(minutes.entrySet());
        Collections.sort(list, Comparator.comparing(Map.Entry::getValue));
        int[] answer = new int[k];
        for (int i = 0; i < k; i++) {
            answer[i] = list.get(i).getValue().size();
        }
        return answer;
    }

    public static void main(String[] args) {
        DescriptionOnly solution = new DescriptionOnly();
        int[][] logs = {{1, 2}, {2, 3}, {4, 5}};
        int k = 3;
        int[] result = solution.describingUserMinutes(logs, k);
        for (int i : result) {
            System.out.print(i + " ");
        }
    }
}