```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExampleTest {

    @Test
    public void test_1() {
        int[][] logs = {{0,5},{1,2},{0,2},{0,5},{1,3}};
        int k = 5;
        int[] answer = new DescriptionExample().descriptionExample(logs, k);
        assertEquals(new int[]{0,2,0,0,0}, answer);
    }

    @Test
    public void test_2() {
        int[][] logs = {{1,1},{2,2},{2,3}};
        int k = 4;
        int[] answer = new DescriptionExample().descriptionExample(logs, k);
        assertEquals(new int[]{1,1,0,0}, answer);
    }

    @Test
    public void test_3() {
        int[][] logs = {{0,5},{0,2},{0,2}};
        int k = 5;
        int[] answer = new DescriptionExample().descriptionExample(logs, k);
        assertEquals(new int[]{1,1,1,1,1}, answer);
    }

    @Test
    public void test_4() {
        int[][] logs = {{1,1},{2,2}};
        int k = 5;
        int[] answer = new DescriptionExample().descriptionExample(logs, k);
        assertEquals(new int[]{0,1,0,0,0}, answer);
    }

    @Test
    public void test_5() {
        int[][] logs = {{1,1},{2,2}};
        int k = 4;
        int[] answer = new DescriptionExample().descriptionExample(logs, k);
        assertEquals(new int[]{0,1,0,0}, answer);
    }
}

class DescriptionExample {

    public int[] descriptionExample(int[][] logs, int k) {
        Map<Integer, Integer> map = new HashMap<>();
        for (int i = 0; i < logs.length; i++) {
            map.put(logs[i][1], map.getOrDefault(logs[i][1], 0) + 1);
        }
        List<Integer> list = new ArrayList<>(map.keySet());
        Collections.sort(list);
        int[] answer = new int[k];
        for (int i = 0; i < k; i++) {
            answer[i] = map.getOrDefault(list.get(i), 0);
        }
        return answer;
    }
}
```