package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2785;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2785.Solution;

public class 2785_DescriptionExample_testsTest {

    public void test1() {
        Solution solution = new Solution();
        String s = "lEetcOde";
        String expected = "lEOtcede";
        assertEquals(expected, solution.sortString(s));
    }

    public void test2() {
        Solution solution = new Solution();
        String s = "lYmpH";
        String expected = "lYmpH";
        assertEquals(expected, solution.sortString(s));
    }

    public void test3() {
        Solution solution = new Solution();
        String s = "aabbccde";
        String expected = "aaabbcce";
        assertEquals(expected, solution.sortString(s));
    }

    public void test4() {
        Solution solution = new Solution();
        String s = "abcdeffgh";
        String expected = "abcdefghi";
        assertEquals(expected, solution.sortString(s));
    }

    public void test5() {
        Solution solution = new Solution();
        String s = "aAabbBcCDeEfFgGhH";
        String expected = "aAbBcCeDfFgGhHi";
        assertEquals(expected, solution.sortString(s));
    }
}