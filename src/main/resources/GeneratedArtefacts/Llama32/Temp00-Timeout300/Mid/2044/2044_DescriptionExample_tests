package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {3, 1};
        int expected = 2;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {2, 2, 2};
        int expected = 7;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {3, 2, 1, 5};
        int expected = 6;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {0, 1, 2, 3};
        int expected = 7;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 4, 8};
        int expected = 15;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }
}