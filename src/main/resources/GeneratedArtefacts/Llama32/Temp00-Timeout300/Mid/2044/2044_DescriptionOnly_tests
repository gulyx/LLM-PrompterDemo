package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertThrows;

public class DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 3};
        int expected = 6;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {0, 1, 2, 3};
        int expected = 7;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1, 1, 1, 1};
        int expected = 4;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {0, 0, 0, 0};
        int expected = 0;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {};
        int expected = 0;
        assertEquals(expected, solution.maxBitwiseOr(nums));
    }
}