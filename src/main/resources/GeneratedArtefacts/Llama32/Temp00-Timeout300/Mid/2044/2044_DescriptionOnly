package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2044_DescriptionOnlyTest {

    @Test
    public void test_1() {
        int[] nums = {0, 1};
        assertEquals(2, findMaximumOR(nums));
    }

    @Test
    public void test_2() {
        int[] nums = {3, 10, 5, 25, 2};
        assertEquals(3, findMaximumOR(nums));
    }

    @Test
    public void test_3() {
        int[] nums = {1, 1, 1, 1, 1};
        assertEquals(15, findMaximumOR(nums));
    }

    @Test
    public void test_4() {
        int[] nums = {0, 2, 3};
        assertEquals(3, findMaximumOR(nums));
    }

    @Test
    public void test_5() {
        int[] nums = {1, 2, 3, 4, 5};
        assertEquals(31, findMaximumOR(nums));
    }
}