    @Test
    public void test_1() {
        int n = 4;
        boolean result = canBeExpressedAsSumOfPowersOfThree(n);
        assertTrue(result);
    }

    @Test
    public void test_2() {
        int n = 9;
        boolean result = canBeExpressedAsSumOfPowersOfThree(n);
        assertTrue(result);
    }

    @Test
    public void test_3() {
        int n = 27;
        boolean result = canBeExpressedAsSumOfPowersOfThree(n);
        assertTrue(result);
    }

    @Test
    public void test_4() {
        int n = 30;
        boolean result = canBeExpressedAsSumOfPowersOfThree(n);
        assertFalse(result);
    }

    @Test
    public void test_5() {
        int n = 33;
        boolean result = canBeExpressedAsSumOfPowersOfThree(n);
        assertTrue(result);
    }
}