    @Test
    public void test_3() {
        int[] nums = {1, 1, 1, 1};
        int[][] result = solution(nums);
        assertArrayEquals(new int[][]{{1}, {1}, {1}, {1}}, result);
    }

    @Test
    public void test_4() {
        int[] nums = {1, 2, 3, 4, 5, 6};
        int[][] result = solution(nums);
        assertArrayEquals(new int[][]{{1, 2, 3}, {4, 5, 6}}, result);
    }

    @Test
    public void test_5() {
        int[] nums = {1, 1, 1, 1, 1};
        int[][] result = solution(nums);
        assertArrayEquals(new int[][]{{1}, {1}, {1}, {1}, {1}}, result);
    }
}

