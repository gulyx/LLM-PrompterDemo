import org.junit.Test;
import static org.junit.Assert.assertArrayEquals;

public class DescriptionExample {

    @Test
    public void test_1() {
        int[] nums = {1, 3, 4, 1, 2, 3, 1};
        int[][] result = solution(nums);
        assertArrayEquals(new int[][]{{1, 3, 4, 2}, {1, 3}, {1}}, result);
    }

    @Test
    public void test_2() {
