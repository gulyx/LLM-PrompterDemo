package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2610;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2610.Solution;

public class 2610_DescriptionExample_testsTest {

    Solution solution = new Solution();

    @Test
    public void test1() {
        int[][] result = solution.create2DArray(new int[]{1,3,4,1,2,3,1});
        assertArrayEquals(new int[][]{{1,3,4,2},{1,3},{1}},result);
    }

    @Test
    public void test2() {
        int[][] result = solution.create2DArray(new int[]{1,2,3,4});
        assertArrayEquals(new int[][]{{4,3,2,1}},result);
    }

    @Test
    public void test3() {
        int[][] result = solution.create2DArray(new int[]{1,1,1,1,1,1,1});
        assertArrayEquals(new int[][]{{1},{1},{1},{1},{1},{1},{1}},result);
    }

    @Test
    public void test4() {
        int[][] result = solution.create2DArray(new int[]{1,2,3,4,5,6,7,8});
        assertArrayEquals(new int[][]{{1,2},{3,4},{5,6},{7,8}},result);
    }

    @Test
    public void test5() {
        int[][] result = solution.create2DArray(new int[]{1,1,1,1,1,1,1});
        assertArrayEquals(new int[][]{{1},{1},{1},{1},{1},{1},{1}},result);
    }
}