package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1381;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1381.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1381_DescriptionExample_testsTest {

    @Test
    public void testPush() {
        Solution solution = new Solution(3);
        solution.push(1);
        solution.push(2);
        assertEquals(null, solution.pop());
    }

    @Test
    public void testPop() {
        Solution solution = new Solution(3);
        solution.push(1);
        solution.push(2);
        assertEquals(-1, solution.pop());
    }

    @Test
    public void testIncrement() {
        Solution solution = new Solution(3);
        solution.push(1);
        solution.push(2);
        solution.increment(0, 100);
        assertEquals(null, solution.pop());
    }

    @Test
    public void testPushAndPop() {
        Solution solution = new Solution(3);
        solution.push(1);
        solution.push(2);
        solution.push(3);
        assertEquals(-1, solution.pop());
    }

    @Test
    public void testIncrementAllElements() {
        Solution solution = new Solution(3);
        solution.increment(0, 100);
        solution.increment(0, 200);
        assertEquals(null, solution.pop());
    }
}