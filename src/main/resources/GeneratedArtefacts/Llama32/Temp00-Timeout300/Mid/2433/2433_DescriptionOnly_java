package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2433;

public class Solution {
    public int[] findArray(int[] pref) {
        int n = pref.length;
        int[] arr = new int[n];
        for (int i = 0; i < n; i++) {
            arr[i] = pref[i] ^ ((pref[i] >> 1) & 0xFFFFFFFF);
        }
        return arr;
    }
}