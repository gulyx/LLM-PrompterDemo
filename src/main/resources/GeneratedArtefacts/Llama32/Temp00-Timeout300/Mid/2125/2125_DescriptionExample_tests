```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2125;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2125.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String[] bank = {"011001","000000","010100","001000"};
        int result = solution.numLaserBeams(bank);
        assertEquals(8, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String[] bank = {"000","111","000"};
        int result = solution.numLaserBeams(bank);
        assertEquals(0, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String[] bank = {"10101","01010","10011","01111"}
        int result = solution.numLaserBeams(bank);
        assertEquals(4, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String[] bank = {"0000","1111","0000","1111"}
        int result = solution.numLaserBeams(bank);
        assertEquals(0, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String[] bank = {"10101","01010","10011","01111","00000","11111"}
        int result = solution.numLaserBeams(bank);
        assertEquals(6, result);
    }
}
```