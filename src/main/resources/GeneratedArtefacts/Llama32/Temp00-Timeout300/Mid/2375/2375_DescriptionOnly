package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 2375_DescriptionOnly {

    @Test
    public void test_1() {
        String pattern = "ID";
        String num = "1239";
        assertEquals(num, lexicographicallySmallestString(pattern));
    }

    @Test
    public void test_2() {
        String pattern = "DD";
        String num = "98";
        assertEquals(num, lexicographicallySmallestString(pattern));
    }

    @Test
    public void test_3() {
        String pattern = "II";
        String num = "12";
        assertEquals(num, lexicographicallySmallestString(pattern));
    }

    @Test
    public void test_4() {
        String pattern = "IDD";
        String num = "1239";
        assertEquals(num, lexicographicallySmallestString(pattern));
    }

    @Test
    public void test_5() {
        String pattern = "DDI";
        String num = "98";
        assertEquals(num, lexicographicallySmallestString(pattern));
    }
}