package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2375;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2375.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 2375_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String pattern = "ID";
        String num = solution.descriptionOnly(pattern);
        assertEquals("12", num);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String pattern = "DD";
        String num = solution.descriptionOnly(pattern);
        assertEquals("98", num);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String pattern = "II";
        String num = solution.descriptionOnly(pattern);
        assertEquals("19", num);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String pattern = "IDD";
        String num = solution.descriptionOnly(pattern);
        assertEquals("1298", num);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String pattern = "DDII";
        String num = solution.descriptionOnly(pattern);
        assertEquals("1989", num);
    }
}