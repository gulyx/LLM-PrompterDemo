package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657.Solution;

public class DescriptionExampleTestsTest {

    public void testPrefixCommonArray() {
        Solution solution = new Solution();
        int[] A = {1, 3, 2, 4};
        int[] B = {3, 1, 2, 4};
        int[] expected = {0, 2, 3, 4};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    public void testPrefixCommonArray_2() {
        Solution solution = new Solution();
        int[] A = {2, 3, 1};
        int[] B = {3, 1, 2};
        int[] expected = {0, 1, 3};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    public void testPrefixCommonArray_3() {
        Solution solution = new Solution();
        int[] A = {1, 2, 3};
        int[] B = {4, 5, 6};
        int[] expected = {};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    public void testPrefixCommonArray_4() {
        Solution solution = new Solution();
        int[] A = {1, 2, 3, 4, 5};
        int[] B = {1, 2, 3, 4, 5};
        int[] expected = {};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    public void testPrefixCommonArray_5() {
        Solution solution = new Solution();
        int[] A = {1, 2, 3};
        int[] B = {4, 5, 6};
        int[] expected = {};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }
}