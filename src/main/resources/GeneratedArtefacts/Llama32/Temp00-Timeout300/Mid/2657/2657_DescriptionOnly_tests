package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class 2657_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] A = {1,2,3};
        int[] B = {3,2,1};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{2, 1, 0}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] A = {1,1,2};
        int[] B = {1,2,2};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{3, 0, 0}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] A = {1,2,3,4,5,6};
        int[] B = {6,5,4,3,2,1};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{6, 5, 4, 3, 2, 1}, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] A = {1,2,3};
        int[] B = {1,2,3,4};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{2, 1, 0}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] A = {1,2,3,4,5};
        int[] B = {5,4,3,2,1};
        int[] result = solution.prefixArray(A,B);
        assertArrayEquals(new int[]{5, 4, 3, 2, 1}, result);
    }
}