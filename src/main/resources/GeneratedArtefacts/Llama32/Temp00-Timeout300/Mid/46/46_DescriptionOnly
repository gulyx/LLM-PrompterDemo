```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import java.util.Arrays;
import java.util.List;

public class DescriptionOnlyTest {

    @Test
    public void testGeneratePermutations() {
        int[] nums = {1, 2, 3};
        List<List<Integer>> result = generatePermutations(nums);
        System.out.println(result);
    }

    @Test
    public void testGeneratePermutationsWithOneElement() {
        int[] nums = {1};
        List<List<Integer>> result = generatePermutations(nums);
        System.out.println(result);
    }

    @Test
    public void testGeneratePermutationsWithTwoElements() {
        int[] nums = {1, 2};
        List<List<Integer>> result = generatePermutations(nums);
        System.out.println(result);
    }

    @Test
    public void testGeneratePermutationsWithThreeElements() {
        int[] nums = {1, 2, 3};
        List<List<Integer>> result = generatePermutations(nums);
        System.out.println(result);
    }

    @Test
    public void testGeneratePermutationsWithDuplicateElements() {
        int[] nums = {1, 1, 2};
        List<List<Integer>> result = generatePermutations(nums);
        System.out.println(result);
    }
}
```