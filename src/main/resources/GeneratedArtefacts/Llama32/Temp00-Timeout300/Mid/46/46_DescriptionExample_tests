```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.46;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.46.Solution;
import org.junit.Test;
import java.util.Arrays;
import java.util.List;

public class DescriptionExampleTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {1, 2, 3};
        List<List<Integer>> result = solution.permute(nums);
        assert result.size() == 6;
        for (List<Integer> perm : result) {
            assert Arrays.equals(perm, Arrays.asList(1, 2, 3));
        }
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {0, 1};
        List<List<Integer>> result = solution.permute(nums);
        assert result.size() == 2;
        for (List<Integer> perm : result) {
            assert Arrays.equals(perm, Arrays.asList(0, 1));
        }
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1};
        List<List<Integer>> result = solution.permute(nums);
        assert result.size() == 1;
        for (List<Integer> perm : result) {
            assert Arrays.equals(perm, Arrays.asList(1));
        }
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {2, 3};
        List<List<Integer>> result = solution.permute(nums);
        assert result.size() == 2;
        for (List<Integer> perm : result) {
            assert Arrays.equals(perm, Arrays.asList(2, 3));
        }
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {4, 5};
        List<List<Integer>> result = solution.permute(nums);
        assert result.size() == 2;
        for (List<Integer> perm : result) {
            assert Arrays.equals(perm, Arrays.asList(4, 5));
        }
    }
}
```