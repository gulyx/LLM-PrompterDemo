package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3016;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3016.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        String word = "a";
        int expected = 1;
        assertEquals(expected, solution.minPushCount(word));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        String word = "ab";
        int expected = 2;
        assertEquals(expected, solution.minPushCount(word));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        String word = "ba";
        int expected = 2;
        assertEquals(expected, solution.minPushCount(word));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        String word = "aa";
        int expected = 1;
        assertEquals(expected, solution.minPushCount(word));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        String word = "aaa";
        int expected = 1;
        assertEquals(expected, solution.minPushCount(word));
    }
}