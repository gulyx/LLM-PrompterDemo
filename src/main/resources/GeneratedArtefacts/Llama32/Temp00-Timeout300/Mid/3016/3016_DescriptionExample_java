package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3016;

public class Solution {
    public int minimumPushCount(String word) {
        int[] count = new int[10];
        for (char c : word.toCharArray()) {
            count[c - 'a']++;
        }
        return count[0] + count[1] + count[2] + count[3] + count[4] + count[5] + count[6] + count[7] + count[8] + count[9];
    }

    [--Example 1--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.minimumPushCount("abcde"));
    }

    [--Example 2--]
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.minimumPushCount("xyzxyzxyzxyz"));
    }
}