package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3016;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.3016.Solution;

public class DescriptionExampleTestsTest {

    public void testExample1() {
        Solution solution = new Solution();
        int expected = 5;
        int actual = solution.minPushCount("abcde");
        assert actual == expected;
    }

    public void testExample2() {
        Solution solution = new Solution();
        int expected = 12;
        int actual = solution.minPushCount("xyzxyzxyzxyz");
        assert actual == expected;
    }

    public void testExample3() {
        Solution solution = new Solution();
        int expected = 6;
        int actual = solution.minPushCount("aabbcc");
        assert actual == expected;
    }

    public void testExample4() {
        Solution solution = new Solution();
        int expected = 10;
        int actual = solution.minPushCount("aaaaaa");
        assert actual == expected;
    }

    public void testExample5() {
        Solution solution = new Solution();
        int expected = 7;
        int actual = solution.minPushCount("abccba");
        assert actual == expected;
    }
}