public class 1769_DescriptionOnly {

    @Test
    public void test1() {
        int[] result = minOperations("001100");
        int[] expected = {0, 1, 2, 3};
        assertArrayEquals(expected, result);
    }

    @Test
    public void test2() {
        int[] result = minOperations("00000");
        int[] expected = {0, 0, 0, 0, 0};
        assertArrayEquals(expected, result);
    }

    @Test
    public void test3() {
        int[] result = minOperations("11111");
        int[] expected = {0, 1, 2, 3, 4};
        assertArrayEquals(expected, result);
    }

    @Test
    public void test4() {
        int[] result = minOperations("01010");
        int[] expected = {0, 2, 2, 0, 0};
        assertArrayEquals(expected, result);
    }

    @Test
    public void test5() {
        int[] result = minOperations("000011");
        int[] expected = {0, 1, 2, 3, 4, 0};
        assertArrayEquals(expected, result);
    }
}