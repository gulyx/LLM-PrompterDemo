package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1409;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1409.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class 1409_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] queries = {2, 1, 3};
        int[] result = solution.descriptionOnlyTests(queries);
        assertArrayEquals(new int[]{2, 3, 1}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] queries = {4, 1, 2, 3};
        int[] result = solution.descriptionOnlyTests(queries);
        assertArrayEquals(new int[]{4, 1, 3, 2}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] queries = {1, 5, 3};
        int[] result = solution.descriptionOnlyTests(queries);
        assertArrayEquals(new int[]{1, 3, 5}, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] queries = {2, 7, 8};
        int[] result = solution.descriptionOnlyTests(queries);
        assertArrayEquals(new int[]{2, 8, 7}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] queries = {1, 2, 3, 4, 5};
        int[] result = solution.descriptionOnlyTests(queries);
        assertArrayEquals(new int[]{1, 2, 3, 4, 5}, result);
    }
}