```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1409;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1409.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExampleTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] queries = {3, 1, 2, 1};
        int m = 5;
        int[] result = solution.processQueries(queries, m);
        assertArrayEquals(new int[]{2, 1, 2, 1}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] queries = {4, 1, 2, 2};
        int m = 4;
        int[] result = solution.processQueries(queries, m);
        assertArrayEquals(new int[]{3, 1, 2, 0}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] queries = {7, 5, 5, 8, 3};
        int m = 8;
        int[] result = solution.processQueries(queries, m);
        assertArrayEquals(new int[]{6, 5, 0, 7, 5}, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] queries = {1, 2, 3};
        int m = 3;
        int[] result = solution.processQueries(queries, m);
        assertArrayEquals(new int[]{0, 1, 2}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] queries = {10, 20, 30};
        int m = 3;
        int[] result = solution.processQueries(queries, m);
        assertArrayEquals(new int[]{2, 1, 0}, result);
    }
}
