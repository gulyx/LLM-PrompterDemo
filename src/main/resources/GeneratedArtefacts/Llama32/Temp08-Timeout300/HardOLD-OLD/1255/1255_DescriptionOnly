package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 1255_DescriptionOnly {

    @Test
      public void test1(){
        String words[] = {"cat", "bt", "hat", "tree"};
        char letters[] = {'a', 'b', 'c'};
        int score[] = {3, 10, 8, 4};

        int result = maxScore(words, letters, score);
        assertEquals(12, result);

    }

     @Test
      public void test2(){
        String words[] = {"cat", "bt", "hat"};
        char letters[] = {'a', 'b'};
        int score[] = {3, 10, 8};

        int result = maxScore(words, letters, score);
        assertEquals(13, result);

    }

     @Test
      public void test3(){
        String words[] = {"cat", "bt", "hat"};
        char letters[] = {'a', 'b'};
        int score[] = {3, 10, 0};

        int result = maxScore(words, letters, score);
        assertEquals(13, result);

    }

     @Test
      public void test4(){
        String words[] = {"cat", "bt", "hat"};
        char letters[] = {'a', 'b'};
        int score[] = {3, 10};

        int result = maxScore(words, letters, score);
        assertEquals(13, result);

    }

     @Test
      public void test5(){
        String words[] = {"cat", "bt", "hat"};
        char letters[] = {'a', 'b'};
        int score[] = {3, 0};

        int result = maxScore(words, letters, score);
        assertEquals(3, result);

    }
}