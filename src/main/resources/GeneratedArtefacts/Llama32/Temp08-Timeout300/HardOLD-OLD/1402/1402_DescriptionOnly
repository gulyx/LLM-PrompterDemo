package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class '1402_DescriptionOnly' {
 
    @Test
    public void test1() {
        int[] satisfaction = { 1, -1, 3, -3 };
        assertEquals(2, solve(satisfaction));
    }

    @Test
    public void test2() {
        int[] satisfaction = { 5 };
        assertEquals(5, solve(satisfaction));
    }

    @Test
    public void test3() {
        int[] satisfaction = { -1, -1, -1, -1, -1 };
        assertEquals(-5, solve(satisfaction));
    }

    @Test
    public void test4() {
        int[] satisfaction = { 0, 0, 0, 0, 0 };
        assertEquals(0, solve(satisfaction));
    }

    @Test
    public void test5() {
        int[] satisfaction = { -1000, -1000, -1000, -1000, -1000 };
        assertEquals(-500000, solve(satisfaction));
    }
}