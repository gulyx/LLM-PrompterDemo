package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.950;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.950.Solution;

public class 950_DescriptionExample_testsTest {

    public void test_1() {
        Solution solution = new Solution();
        int[] deck = {17,13,11,2,3,5,7};
        int[] expectedOutput = {2,13,3,11,5,17,7};
        assertArrayEquals(expectedOutput, solution.createDeck(deck));
    }

    public void test_2() {
        Solution solution = new Solution();
        int[] deck = {1,1000};
        int[] expectedOutput = {1,1000};
        assertArrayEquals(expectedOutput, solution.createDeck(deck));
    }

    public void test_3() {
        Solution solution = new Solution();
        int[] deck = {5, 2, 8, 9, 10, 11, 12, 1, 6, 7};
        int[] expectedOutput = {2,1,8,9,10,11,12,5,6,7};
        assertArrayEquals(expectedOutput, solution.createDeck(deck));
    }

    public void test_4() {
        Solution solution = new Solution();
        int[] deck = {4, 3, 2, 1, 5};
        int[] expectedOutput = {1,2,3,4,5};
        assertArrayEquals(expectedOutput, solution.createDeck(deck));
    }

    public void test_5() {
        Solution solution = new Solution();
        int[] deck = {10, 15, 20, 25, 30, 35, 40, 45, 50, 55};
        int[] expectedOutput = {10,15,20,25,30,35,40,45,50,55};
        assertArrayEquals(expectedOutput, solution.createDeck(deck));
    }

}