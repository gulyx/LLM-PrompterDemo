package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2657.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] A = {1, 3, 2, 4};
        int[] B = {3, 1, 2, 4};
        int[] expected = {0, 2, 3, 4};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] A = {2, 3, 1};
        int[] B = {3, 1, 2};
        int[] expected = {0, 1, 3};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] A = {4, 5, 2, 1, 7, 3, 6};
        int[] B = {2, 1, 8, 4, 3, 7, 6};
        int[] expected = {0, 1, 3, 4, 5, 5, 5};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] A = {1, 2, 3, 4, 5};
        int[] B = {5, 4, 3, 2, 1};
        int[] expected = {0, 1, 2, 3, 4};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] A = {1, 1, 1, 1, 1};
        int[] B = {1, 1, 1, 1, 1};
        int[] expected = {0, 1, 2, 3, 4};
        int[] actual = solution.prefixCommonArray(A, B);
        assertArrayEquals(expected, actual);
    }
}