package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1669;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1669.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1669_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        ListNode node3 = new ListNode(3);
        ListNode node5 = new ListNode(5);
        ListNode node6 = new ListNode(6);
        ListNode node8 = new ListNode(8);
        ListNode node9 = new ListNode(9);

        ListNode result = solution.mergeInBetween(node3, 0, 2);
        assertEquals(result.val, 6);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        ListNode node1 = new ListNode(1);
        ListNode node2 = new ListNode(2);
        ListNode node3 = new ListNode(3);
        ListNode node4 = new ListNode(4);
        ListNode node5 = new ListNode(5);

        ListNode result = solution.mergeInBetween(node1, 1, 3);
        assertEquals(result.val, 4);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        ListNode node0 = new ListNode(0);
        ListNode node1 = new ListNode(1);
        ListNode node2 = new ListNode(2);

        ListNode result = solution.mergeInBetween(node0, 0, 1);
        assertEquals(result.val, 2);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        ListNode node1 = new ListNode(1);
        ListNode node2 = new ListNode(2);
        ListNode node3 = new ListNode(3);
        ListNode node4 = new ListNode(4);

        ListNode result = solution.mergeInBetween(node1, 0, 3);
        assertEquals(result.val, 3);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        ListNode node1 = new ListNode(1);
        ListNode node2 = new ListNode(2);

        ListNode result = solution.mergeInBetween(node1, 0, 2);
        assertEquals(result.val, 2);
    }
}
