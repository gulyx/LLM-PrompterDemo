package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

public class Test_2044_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {0, 1};
        assertEquals(2, solution.descriptionOnly(nums));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {0, 0};
        assertEquals(2, solution.descriptionOnly(nums));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1, 1, 0};
        assertEquals(4, solution.descriptionOnly(nums));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {0, 1, 2};
        assertEquals(7, solution.descriptionOnly(nums));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {0, 0, 0};
        assertEquals(1, solution.descriptionOnly(nums));
    }
}