package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2044.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class 2044_DescriptionExample_testsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] nums = {3,1};
        assertEquals(2, solution.maxOrSubsets(nums));
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] nums = {2,2,2};
        assertEquals(7, solution.maxOrSubsets(nums));
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] nums = {3,2,1,5};
        assertEquals(6, solution.maxOrSubsets(nums));
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] nums = {0,1,2,3,4};
        assertEquals(15, solution.maxOrSubsets(nums));
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[] nums = {};
        assertEquals(0, solution.maxOrSubsets(nums));
    }
}