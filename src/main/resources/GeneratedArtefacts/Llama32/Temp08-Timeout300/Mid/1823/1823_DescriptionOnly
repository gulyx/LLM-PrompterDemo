package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1823_DescriptionOnly {

    @Test
    public void test_n_k_equal() {
        int result = circleGame(5, 1);
        assertEquals(5, result);
    }

    @Test
    public void test_k_greater_than_n() {
        int result = circleGame(5, 6);
        assertEquals(-1, result);
    }

    @Test
    public void test_k_less_than_1() {
        int result = circleGame(5, 0);
        assertEquals(-1, result);
    }

    @Test
    public void test_n_equal_to_k() {
        int result = circleGame(1, 2);
        assertEquals(-1, result);
    }

    @Test
    public void test_n_equal_to_1() {
        int result = circleGame(1, 3);
        assertEquals(1, result);
    }
}