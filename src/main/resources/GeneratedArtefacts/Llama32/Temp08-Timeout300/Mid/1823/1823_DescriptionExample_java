```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1823;

public class Solution {
    public int getWinner(int n, int k) {
        for (int i = 0; i < n; i++) {
            if ((i + 1 + k) % n == 0) {
                return i + 1;
            }
        }
        return 1;
    }

    // --Example 1--
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.getWinner(5, 2)); // Output: 3
    }

    // --Example 2--
    public static void main(String[] args) {
        Solution solution = new Solution();
        System.out.println(solution.getWinner(6, 5)); // Output: 1
    }
}
```