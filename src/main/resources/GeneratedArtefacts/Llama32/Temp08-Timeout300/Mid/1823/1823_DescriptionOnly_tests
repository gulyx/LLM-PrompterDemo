package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1823_DescriptionOnlyTest {

    @Test
    public void testWinner() {
        int result = findWinner(5,2);
        assertEquals(5, result);
    }

    @Test
    public void testWinner_SameNumber() {
        int result = findWinner(3,1);
        assertEquals(3, result);
    }

    @Test
    public void testWinner_MultipleNumbers() {
        int result = findWinner(4,2);
        assertEquals(4, result);
    }

    @Test
    public void testWinner_EqualNumbers() {
        int result = findWinner(5,5);
        assertEquals(5, result);
    }

    @Test
    public void testWinner_LargeInputs() {
        int result = findWinner(500,250);
        assertEquals(500, result);
    }
}