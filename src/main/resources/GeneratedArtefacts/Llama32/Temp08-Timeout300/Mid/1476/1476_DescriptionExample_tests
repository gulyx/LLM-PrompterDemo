package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1476;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1476.Solution;
import org.junit.jupiter.api.Test;

public class 1476_DescriptionExample_testsTest {

    @Test
    public void test01() {
        Solution solution = new Solution(new int[][]{{1,2,1},{4,3,4},{3,2,1},{1,1,1}});
        solution.updateSubrectangle(0, 2, 0, 2, 5);
        solution.getValue(3, 1);
    }

    @Test
    public void test02() {
        Solution solution = new Solution(new int[][]{{1,2,1},{4,3,4},{3,2,1},{1,1,1}});
        solution.updateSubrectangle(0, 2, 0, 2, 10);
        solution.getValue(3, 1);
    }

    @Test
    public void test03() {
        Solution solution = new Solution(new int[][]{{1,1,1},{2,2,2},{3,3,3}});
        solution.updateSubrectangle(0, 0, 0, 2, 100);
        solution.getValue(0, 0);
    }

    @Test
    public void test04() {
        Solution solution = new Solution(new int[][]{{1,1,1},{2,2,2},{3,3,3}});
        solution.updateSubrectangle(0, 0, 0, 2, 20);
        solution.getValue(2, 2);
    }

    @Test
    public void test05() {
        Solution solution = new Solution(new int[][]{{1,2,1},{4,3,4},{3,2,1},{1,1,1}});
        solution.updateSubrectangle(0, 2, 0, 2, 5);
        solution.getValue(3, 1);
    }
}