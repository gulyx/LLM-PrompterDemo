package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import java.util.Arrays;

public class 1877_DescriptionExampleTest {

    @Test
    public void testMaxPairSum() {
        int[] nums = {3,5,2,3};
        int result = maxPairSum(nums);
        assert result == 7;
    }

    @Test
    public void testMaxPairSum_1() {
        int[] nums = {3,5,4,2,4,6};
        int result = maxPairSum(nums);
        assert result == 8;
    }

    @Test
    public void testMaxPairSum_2() {
        int[] nums = {1,2,3,4};
        int result = maxPairSum(nums);
        assert result == 7;
    }

    @Test
    public void testMaxPairSum_3() {
        int[] nums = {1,1,1,1};
        int result = maxPairSum(nums);
        assert result == 2;
    }

    @Test
    public void testMaxPairSum_4() {
        int[] nums = {10,20,30,40};
        int result = maxPairSum(nums);
        assert result == 60;
    }
}