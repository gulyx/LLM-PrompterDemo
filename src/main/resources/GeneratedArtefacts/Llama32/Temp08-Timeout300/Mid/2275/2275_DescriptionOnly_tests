package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2275;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2275.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 2275_DescriptionOnly_testsTest {

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[][] candidates1= {{1,2,3}};
        assertEquals(2,solution.maxBitwiseAnd(candidates1));
    }

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[][] candidates1= {{2,4,8},{1,3,7},{6,5}};
        assertEquals(3,solution.maxBitwiseAnd(candidates1));
    }

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[][] candidates1= {{15, 10, 5}, {20, 30, 40} };
        assertEquals(0, solution.maxBitwiseAnd(candidates1));
    }

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[][] candidates1= {{100000}},{{2}};
        assertEquals(2,solution.maxBitwiseAnd(candidates1));
    }

    @Test
    public void test_Solution() {
        Solution solution = new Solution();
        int[][] candidates1= {{1, 2}, {3,4}, {5}};int ans=solution.maxBitwiseAnd(candidates1);
        assertEquals(0,solution.maxBitwiseAnd(new int[][]{{ans}}));
    }

}