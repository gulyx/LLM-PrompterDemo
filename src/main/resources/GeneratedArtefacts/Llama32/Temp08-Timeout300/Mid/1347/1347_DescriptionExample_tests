```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1347;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1347.Solution;

public class SolutionTest {

    public static void main(String[] args) {
        org.junit.Test
        public void test1() {
            Solution solution = new Solution();
            int actual = solution.minSteps("bab", "aba");
            org.junit.Assert.assertEquals(1, actual);
        }

        @Test
        public void test2() {
            Solution solution = new Solution();
            int actual = solution.minSteps("leetcode", "practice");
            org.junit.Assert.assertEquals(5, actual);
        }

        @Test
        public void test3() {
            Solution solution = new Solution();
            int actual = solution.minSteps("anagram", "mangaar");
            org.junit.Assert.assertEquals(0, actual);
        }

        @Test
        public void test4() {
            Solution solution = new Solution();
            int actual = solution.minSteps("hello", "world");
            org.junit.Assert.assertEquals(-1, actual); // or any other default value
        }

        @Test
        public void test5() {
            Solution solution = new Solution();
            int actual = solution.minSteps("", "");
            org.junit.Assert.assertEquals(0, actual);
        }
    }
}
```