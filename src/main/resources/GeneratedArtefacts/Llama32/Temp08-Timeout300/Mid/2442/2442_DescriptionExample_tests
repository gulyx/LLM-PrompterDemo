package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2442;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2442.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class descriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {1,13,10,12,31};
        int result = solution.numDistinct(nums);
        assertEquals(6, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {2,2,2};
        int result = solution.numDistinct(nums);
        assertEquals(1, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {5,0,1,9};
        int result = solution.numDistinct(nums);
        assertEquals(4, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {7,7,7,7,7,9};
        int result = solution.numDistinct(nums);
        assertEquals(3, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {};
        int result = solution.numDistinct(nums);
        assertEquals(0, result);
    }
}
