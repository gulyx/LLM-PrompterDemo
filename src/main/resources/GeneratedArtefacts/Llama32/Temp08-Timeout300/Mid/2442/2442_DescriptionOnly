package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertTrue;

public class 2442_DescriptionOnly {

    @Test
   public void test_1() {
       int[] nums = {100,200};
       int result = new Solution().findDifferentNumbers(nums);
       assertEquals(3, result);
   }

   @Test 
   public void test_2() {
        int[] nums = {1000, 2000, 3,4,5};
        int result = new Solution().findDifferentNumbers(nums);
        assertTrue(result==9);    
    }
      
     @Test
     public void test_3() {
       int[] nums = {1,2,3,4,5,10};
       int result = new Solution().findDifferentNumbers(nums);
       assertEquals(15, result);
   }

     @Test 
      public void test_4() {
         int[] nums = {13, 17, 23, 30, 34, 91};
        int result = new Solution().findDifferentNumbers(nums);
        assertTrue(result==7);    
    }
     
     @Test
     public void test_5() {
       int[] nums = {10,100,500,1000,2000};
       int result = new Solution().findDifferentNumbers(nums);
       assertEquals(6, result);
   }

}
