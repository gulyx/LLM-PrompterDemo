package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.46;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.46.Solution;

public class 46_DescriptionOnly_testsTest {

    @org.junit.Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = { 1, 2, 3 };
        org.junit.Assert.assertArrayEquals(new int[]{new int[]{1, 2, 3}, new int[]{1, 3, 2}, new int[]{2, 1, 3}, new int[]{2, 3, 1}, new int[]{3, 1, 2}, new int[]{3, 2, 1}}, solution.permute(nums));
    }

    @org.junit.Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = { 1 };
        org.junit.Assert.assertArrayEquals(new int[][]{new int[]{1}}), solution.permute(nums));
    }

    @org.junit.Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = { 2, 2, 3, 1 };
        org.junit.Assert.assertArrayEquals(new int[][]{
            new int[]{1, 2, 2, 3},
            new int[]{1, 2, 3, 2},
            new int[]{1, 3, 2, 2},
            new int[]{2, 1, 2, 3},
            new int[]{2, 1, 3, 2},
            new int[]{2, 2, 1, 3},
            new int[]{2, 2, 3, 1},
            new int[]{2, 3, 1, 2},
            new int[]{2, 3, 2, 1},
            new int[]{3, 1, 2, 2},
            new int[]{3, 1, 2, 2},
            new int[]{3, 2, 1, 2},
            new int[]{3, 2, 2, 1}
        }, solution.permute(nums));
    }

    @org.junit.Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = { 5 };
        org.junit.Assert.assertArrayEquals(new int[][]{new int[]{5}}), solution.permute(nums));
    }

    @org.junit.Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {};
        org.junit.Assert.assertTrue(solution.permute(nums).length == 0);
    }
}