package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2149;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2149.Solution;

public class 2149_DescriptionOnly_testsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] nums = {4, -4};
        int[] result = solution.getArray(nums);
        assertArrayEquals(new int[]{4, -4}, result);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] nums = {-1, 1, -1, 1, -1, 1};
        int[] result = solution.getArray(nums);
        assertArrayEquals(new int[]{1, -1, 1, -1, 1, -1}, result);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] nums = {1, -1, 1, -1};
        int[] result = solution.getArray(nums);
        assertArrayEquals(new int[]{1, 1, -1, -1}, result);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] nums = {2, -2, 2, -2};
        int[] result = solution.getArray(nums);
        assertArrayEquals(new int[]{2, 2, -2, -2}, result);
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[] nums = {-1, 1, -3, 2, -4};
        int[] result = solution.getArray(nums);
        assertArrayEquals(new int[]{1, -1, 2, -3, -4}, result);
    }
}