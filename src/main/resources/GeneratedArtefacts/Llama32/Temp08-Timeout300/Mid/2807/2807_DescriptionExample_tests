package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2807;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2807.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 2807_DescriptionExample_testsTest {

    @Test
    public void testInsertGCD() {
        Solution solution = new Solution();
        ListNode head = new ListNode(18);
        head.next = new ListNode(6);
        head.next.next = new ListNode(10);
        head.next.next.next = new ListNode(3);
        ListNode result = solution.insertGCD(head, 5);
        while (result != null) {
            System.out.println(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDNoChange() {
        Solution solution = new Solution();
        ListNode head = new ListNode(7);
        ListNode result = solution.insertGCD(head, 1);
        while (result != null) {
            System.out.println(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDSingleNode() {
        Solution solution = new Solution();
        ListNode head = new ListNode(7);
        ListNode result = solution.insertGCD(head, 2);
        while (result != null) {
            System.out.println(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDNoChangeSingleNode() {
        Solution solution = new Solution();
        ListNode head = new ListNode(7);
        ListNode result = solution.insertGCD(head, 1);
        while (result != null) {
            System.out.println(result.val + " ");
            result = result.next;
        }
    }

    @Test
    public void testInsertGCDWithGCD() {
        Solution solution = new Solution();
        ListNode head = new ListNode(6);
        head.next = new ListNode(10);
        head.next.next = new ListNode(3);
        ListNode result = solution.insertGCD(head, 5);
        while (result != null) {
            System.out.println(result.val + " ");
            result = result.next;
        }
    }
}