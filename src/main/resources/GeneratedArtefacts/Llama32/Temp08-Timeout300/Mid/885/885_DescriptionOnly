package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 885_DescriptionOnly {

    @Test
    public void test_case_1() {
        int[][] result = new int[0][2];
        int[] expected = { 1, 3 };
        assertArrayEquals(result, SpiralMatrix(2, 2), 0.0);
    }

    @Test
    public void test_case_2() {
        int[][] result = new int[8][2];
        int[] expected = { 1, 2, 3, 4, 11, 12, 15, 16 };
        assertArrayEquals(result, SpiralMatrix(5, 5), 0.0);
    }

    @Test
    public void test_case_3() {
        int[][] result = new int[8][2];
        int[] expected = { 1, 4, 7, 6, 9, 10, 15, 16 };
        assertArrayEquals(result, SpiralMatrix(5, 5), 0.0);
    }

    @Test
    public void test_case_4() {
        int[][] result = new int[8][2];
        int[] expected = { 1, 3, 7, 9, 15, 17, 24, 26 };
        assertArrayEquals(result, SpiralMatrix(5, 5), 0.0);
    }

    @Test
    public void test_case_5() {
        int[][] result = new int[16][2];
        int[] expected = {1, 3, 7, 9, 15, 17,24,26,27,29,30,31,34,37,38,39};
        assertArrayEquals(result, SpiralMatrix(6, 6), 0.0);
    }

}