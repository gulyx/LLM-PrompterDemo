package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1630;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1630.Solution;

public class 1630_DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] nums = {4,6,5,9,3,7};
        int[][] l = {{0},{0},{2}};
        int[][] r = {{2},{3},{5}};

        boolean[] result = solution.checkRangeNumberThree(nums, l, r);
        assertArrayEquals(new boolean[]{true,false,true}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] nums = {-12,-9,-3,-12,-6,15,20,-25,-20,-15,-10};
        int[][] l = {{0},{1},{6},{4},{8},{7}};

        boolean[] result = solution.checkRangeNumberThree(nums, l, null);
        assertArrayEquals(new boolean[]{false,true,false,false,true,true}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5};
        int[][] l = {{0},{0},{0}};

        boolean[] result = solution.checkRangeNumberThree(nums, l, null);
        assertArrayEquals(new boolean[]{true,true,true}, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] nums = {1,3,5};
        int[][] l = {{0},{0},{0}};
        int[][] r = {{2},{3},{4}};

        boolean[] result = solution.checkRangeNumberThree(nums, null, r);
        assertArrayEquals(new boolean[]{true,true,false}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] nums = {1,2,3,4,5};
        int[][] l = {{0},{0},{0}};
        int[][] r = {{0},{0},{0}};

        boolean[] result = solution.checkRangeNumberThree(nums, l, r);
        assertArrayEquals(new boolean[]{true,true,false}, result);
    }
}