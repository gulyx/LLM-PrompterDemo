package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1605_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[] rowSum = {30,11,23};
        int[] colSum = {15,21,17};
        int[][] result = findMatrix(rowSum,colSum);
        assertNotNull(result);

    }

    @Test
    public void test2() {
        int[] rowSum = {1,2};
        int[] colSum = {3,4};
        int[][] result = findMatrix(rowSum,colSum);
        assertArrayEquals(new int[][]{{1,4},{2,3}},result);

    }

    @Test
    public void test3() {
        int[] rowSum = {10,20};
        int[] colSum = {5,15};
        int[][] result = findMatrix(rowSum,colSum);
        assertArrayEquals(new int[][]{{1,6},{2,9}},result);

    }

    @Test
    public void test4() {
        int[] rowSum = {10,20};
        int[] colSum = {5,15};
        int[][] result = findMatrix(rowSum,colSum);
        assertArrayEquals(new int[][]{{1,6},{2,9}},result);

    }

    @Test
    public void test5() {
        int[] rowSum = {10,20};
        int[] colSum = {5,15};
        int[][] result = findMatrix(rowSum,colSum);
        assertArrayEquals(new int[][]{{1,6},{2,9}},result);

