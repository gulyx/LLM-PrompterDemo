```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescritionExampleTest {

    @Test
    public void testExample1() {
        int[][] rowSum = {3, 8};
        int[][] colSum = {4, 7};
        int[][] result = new DescriptionExample().example1(rowSum, colSum);
        assertEquals(new int[][] {{3, 0}, {1, 7}}, result);
    }

    @Test
    public void testExample2() {
        int[][] rowSum = {5, 7, 10};
        int[][] colSum = {8, 6, 8};
        int[][] result = new DescriptionExample().example2(rowSum, colSum);
        assertEquals(new int[][] {{0, 5, 0}, {6, 1, 0}, {2, 0, 8}}, result);
    }

    @Test
    public void testExample3() {
        int[][] rowSum = {9, 10};
        int[][] colSum = {9, 11};
        int[][] result = new DescriptionExample().example3(rowSum, colSum);
        assertArrayEquals(new int[][] {{1, 8}, {8, 1}}, result);
    }

    @Test
    public void testExample4() {
        int[][] rowSum = {1, 2};
        int[][] colSum = {3, 4};
        int[][] result = new DescriptionExample().example4(rowSum, colSum);
        assertArrayEquals(new int[][] {{0, 0}, {1, 0}}, result);
    }

    @Test
    public void testExample5() {
        int[][] rowSum = {10};
        int[][] colSum = {10};
        int[][] result = new DescriptionExample().example5(rowSum, colSum);
        assertArrayEquals(new int[][] {{5, 5}, {5, 0}}, result);
    }
}
```