```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1605;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1605.Solution;
import org.junit.Test;
import static org.junit.Assert.assertArrayEquals;

public class 1605_DescriptionExample_testsTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[][] rowSum = {{3,8}};
        int[][] colSum = {{4,7}};
        int[][] result = solution.restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{3,0},{1,7}},result);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[][] rowSum = {{5,7,10}};
        int[][] colSum = {{8,6,8}};
        int[][] result = solution.restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{0,5,0},{6,1,0},{2,0,8}},result);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[][] rowSum = {{15}};
        int[][] colSum = {{20}};
        int[][] result = solution.restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{15,0},{0,5}},result);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[][] rowSum = {{10,6}};
        int[][] colSum = {{3,7}};
        int[][] result = solution.restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{0,2},{3,1}},result);
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[][] rowSum = {{15,10}};
        int[][] colSum = {{8,8}};
        int[][] result = solution.restoreMatrix(rowSum, colSum);
        assertArrayEquals(new int[][]{{2,4},{3,0}},result);
    }
}
```