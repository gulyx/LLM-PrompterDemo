package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2545;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2545.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 2545_DescriptionExample_testsTest {
    @Test
    public void test1(){
        Solution solution = new Solution();
        int[][] score = { {10,6,9,1},{7,5,11,2},{4,8,3,15}};
        int k = 2;
        int[][] result = solution.sortStudents(score,k);
        assertEquals(Arrays.deepToString(new int[][]{{7,5,11,2},{10,6,9,1},{4,8,3,15}}), Arrays.deepToString(result));
    }

    @Test
    public void test2(){
        Solution solution = new Solution();
        int[][] score = { {3,4},{5,6}};
        int k = 0;
        int[][] result = solution.sortStudents(score,k);
        assertEquals(Arrays.deepToString(new int[][]{{5,6},{3,4}}), Arrays.deepToString(result));
    }

    @Test
    public void test3(){
        Solution solution = new Solution();
        int[][] score = { {1,2,3},{4,5,6}};
        int k = 1;
        int[][] result = solution.sortStudents(score,k);
        assertEquals(Arrays.deepToString(new int[][]{{4,5,6},{1,2,3}}), Arrays.deepToString(result));
    }

    @Test
    public void test4(){
        Solution solution = new Solution();
        int[][] score = { {10,20,30},{40,50,60}};
        int k = 0;
        int[][] result = solution.sortStudents(score,k);
        assertEquals(Arrays.deepToString(new int[][]{{40,50,60},{10,20,30}}), Arrays.deepToString(result));
    }

    @Test
    public void test5(){
        Solution solution = new Solution();
        int[][] score = { {1,2,3},{4,5,6}};
        int k = 2;
        int[][] result = solution.sortStudents(score,k);
        assertEquals(Arrays.deepToString(new int[][]{{4,5,6},{1,2,3}}), Arrays.deepToString(result));
    }
}