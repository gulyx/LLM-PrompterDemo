package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2545;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2545.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertArrayEquals;

public class 2545_DescriptionOnly_testsTest {

    @Test
    public void test1(){
        Solution solution = new Solution();
        int[][] score = {{7,8},{9,1}};
        int k = 0;
        assertArrayEquals(solution.sortStudents(score,k),score);
    }

    @Test
    public void test2(){
        Solution solution = new Solution();
        int[][] score = {{1,1},{3,5},{6,4},{8,8},{10,9},{12,7}};
        int k = 0;
        assertArrayEquals(solution.sortStudents(score,k),score);
    }

    @Test
    public void test3(){
        Solution solution = new Solution();
        int[][] score = {{1,2},{3,5}};
        int k = 1;
        assertArrayEquals(solution.sortStudents(score,k),[[1,2],[3,5]]);
    }

    @Test
    public void test4(){
        Solution solution = new Solution();
        int[][] score = {{1,2},{3,5},{7,6}};
        int k = 0;
        assertArrayEquals(solution.sortStudents(score,k),score);
    }

    @Test
    public void test5(){
        Solution solution = new Solution();
        int[][] score = {{8,9},{7,10},{6,11},{5,12},{4,13},{3,14}};
        int k = 1;
        assertArrayEquals(solution.sortStudents(score,k),score);
    }
}