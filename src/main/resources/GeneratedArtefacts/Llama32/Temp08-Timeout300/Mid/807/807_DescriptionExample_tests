package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTest {

    @Test
    public void test1() {
        int[][] grid = {{3,0,8,4},{2,4,5,7},{9,2,6,3},{0,3,1,0}};
        assertEquals(35, maxSum(grid));
    }

    @Test
    public void test2() {
        int[][] grid = {{0,0,0},{0,0,0},{0,0,0}};
        assertEquals(0, maxSum(grid));
    }

    @Test
    public void test3() {
        int[][] grid = {{100,0,8,4},{2,4,5,7},{9,2,6,3},{0,3,1,0}};
        assertEquals(10000, maxSum(grid));
    }

    @Test
    public void test4() {
        int[][] grid = {{1,1,1},{1,1,1},{1,1,1}};
        assertEquals(9, maxSum(grid));
    }

    @Test
    public void test5() {
        int[][] grid = {{0,100,8,4},{2,4,5,7},{9,2,6,3},{0,3,1,0}};
        assertEquals(10900, maxSum(grid));
    }
}