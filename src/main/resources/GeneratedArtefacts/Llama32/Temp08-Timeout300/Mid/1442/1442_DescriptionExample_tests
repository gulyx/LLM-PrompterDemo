package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1442;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1442.Solution;

public class Test {

    @Test
    public void test1() {
        Solution s = new Solution();
        int[] arr = {2, 3, 1, 6, 7};
        assertEquals(4, s.numOfTriplets(arr));
    }

    @Test
    public void test2() {
        Solution s = new Solution();
        int[] arr = {1, 1, 1, 1, 1};
        assertEquals(10, s.numOfTriplets(arr));
    }

    @Test
    public void test3() {
        Solution s = new Solution();
        int[] arr = {2, 2, 4, 8};
        assertEquals(0, s.numOfTriplets(arr));
    }

    @Test
    public void test4() {
        Solution s = new Solution();
        int[] arr = {1, 2, 3, 4, 5};
        assertEquals(15, s.numOfTriplets(arr));
    }

    @Test
    public void test5() {
        Solution s = new Solution();
        int[] arr = {0, 0, 0, 0, 0};
        assertEquals(10, s.numOfTriplets(arr));
    }
}