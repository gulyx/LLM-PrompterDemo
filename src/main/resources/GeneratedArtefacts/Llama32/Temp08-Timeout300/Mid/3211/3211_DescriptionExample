```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import java.util.Arrays;

public class DescriptionExample {

    @Test
    public void validBinaryStrings_3() {
        int n = 3;
        String[] expected = {"010","011","101","110","111"};
        assertAll(Arrays.asList(expected), actual -> System.out.println(actual));
    }

    @Test
    public void validBinaryStrings_1() {
        int n = 1;
        String[] expected = {"0","1"};
        assertAll(Arrays.asList(expected), actual -> System.out.println(actual));
    }

    @Test
    public void validBinaryStrings_BiggerThan3() {
        int n = 4;
        String[] expected = {"0101", "0110", "1010", "1101"};
        assertAll(Arrays.asList(expected), actual -> System.out.println(actual));
    }

    @Test
    public void validBinaryStrings_EmptyString() {
        int n = 0;
        String[] expected = {};
        assertAll(Arrays.asList(expected), actual -> System.out.println(actual));
    }

    @Test
    public void validBinaryStrings_NoOnes() {
        int n = 5;
        String[] expected = {"00000"};
        assertAll(Arrays.asList(expected), actual -> System.out.println(actual));
    }
}
```