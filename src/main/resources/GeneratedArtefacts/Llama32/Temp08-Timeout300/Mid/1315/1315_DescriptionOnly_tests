package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import java.util.*;

public class DescriptionOnlyTest {

    public void testSumEvenGrandParent() {
        TreeNode root = new TreeNode(3, new TreeNode(5), new TreeNode(9));
        System.out.println(sumEvenGrandParent(root)); // Expected output: 0
    }

    public void testSumEvenGrandParent2() {
        TreeNode root = new TreeNode(3, new TreeNode(4, new TreeNode(8), new TreeNode(10)), new TreeNode(9));
        System.out.println(sumEvenGrandParent(root)); // Expected output: 16
    }

    public void testSumEvenGrandParent3() {
        TreeNode root = new TreeNode(2);
        System.out.println(sumEvenGrandParent(root)); // Expected output: 0
    }

    public void testSumEvenGrandParent4() {
        TreeNode root = new TreeNode(1, new TreeNode(2), new TreeNode(3));
        System.out.println(sumEvenGrandParent(root)); // Expected output: 0
    }

    public void testSumEvenGrandParent5() {
        TreeNode root = new TreeNode(8);
        System.out.println(sumEvenGrandParent(root)); // Expected output: 0
    }
}

class TreeNode {
    int val;
    TreeNode left;
    TreeNode right;
    TreeNode(int x) { val = x; }
}