package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1079_DescriptionOnlyTest {

    @Test
    public void test_0_tiles() {
        int[] tiles = new int[0];
        assertEquals(0, numDistinct(tiles));
    }

    @Test
    public void test_1_tile() {
        int[] tiles = new int[]{0};
        assertEquals(1, numDistinct(tiles));
    }

    @Test
    public void test_2_tiles() {
        int[] tiles = new int[]{0, 1};
        assertEquals(2, numDistinct(tiles));
    }

    @Test
    public void test_3_tiles() {
        int[] tiles = new int[]{0, 0, 1};
        assertEquals(2, numDistinct(tiles));
    }

    @Test
    public void test_4_tiles() {
        int[] tiles = new int[]{0, 0, 0, 1};
        assertEquals(2, numDistinct(tiles));
    }
}