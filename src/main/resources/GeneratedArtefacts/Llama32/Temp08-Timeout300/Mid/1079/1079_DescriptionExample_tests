```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1079;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1079.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class DescriptionExampleTestsTest {

    @Test
    public void testExample1() {
        Solution solution = new Solution();
        String tiles = "AAB";
        int expectedOutput = 8;
        int actualOutput = solution.numTilePossibilities(tiles);
        assertEquals(expectedOutput, actualOutput);
    }

    @Test
    public void testExample2() {
        Solution solution = new Solution();
        String tiles = "AAABBC";
        int expectedOutput = 188;
        int actualOutput = solution.numTilePossibilities(tiles);
        assertEquals(expectedOutput, actualOutput);
    }

    @Test
    public void testExample3() {
        Solution solution = new Solution();
        String tiles = "V";
        int expectedOutput = 1;
        int actualOutput = solution.numTilePossibilities(tiles);
        assertEquals(expectedOutput, actualOutput);
    }

    @Test
    public void testEmptyString() {
        Solution solution = new Solution();
        String tiles = "";
        int expectedOutput = 0;
        int actualOutput = solution.numTilePossibilities(tiles);
        assertEquals(expectedOutput, actualOutput);
    }

    @Test
    public void testAllTilesSameLetter() {
        Solution solution = new Solution();
        String tiles = "AAAA";
        int expectedOutput = 1;
        int actualOutput = solution.numTilePossibilities(tiles);
        assertEquals(expectedOutput, actualOutput);
    }
}
```