```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class Solution {

    @Test
    public void test_1() {
        int[] preorder = {3, 9, 20, 15, 7};
        Solution solution = new Solution();
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void test_2() {
        int[] preorder = {5, 1, 4, null, null, 3, 6, 0};
        Solution solution = new Solution();
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void test_3() {
        int[] preorder = {2, 1, 3, null};
        Solution solution = new Solution();
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void test_4() {
        int[] preorder = {1, 2, 3, 4, 5, 6, 7};
        Solution solution = new Solution();
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNotNull(root);
    }

    @Test
    public void test_5() {
        int[] preorder = {};
        Solution solution = new Solution();
        TreeNode root = solution.bstFromPreorder(preorder);
        assertNull(root);
    }
}
