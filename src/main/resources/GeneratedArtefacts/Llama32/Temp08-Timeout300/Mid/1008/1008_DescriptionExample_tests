package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1008;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1008.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1008_DescriptionExample_testsTest {

    @Test
    public void test_Solution_1() {
        Solution solution = new Solution();
        int[] preorder = {8,5,1,7,10,12};
        Node root = solution.bstFromPreorder(preorder);
        assertEquals(root.val, 8);
    }

    @Test
    public void test_Solution_2() {
        Solution solution = new Solution();
        int[] preorder = {1,3};
        Node root = solution.bstFromPreorder(preorder);
        assertEquals(root.val, 1);
    }

    @Test
    public void test_Solution_3() {
        Solution solution = new Solution();
        int[] preorder = {1,2,3};
        Node root = solution.bstFromPreorder(preorder);
        assertEquals(root.val, 1);
    }

    @Test
    public void test_Solution_4() {
        Solution solution = new Solution();
        int[] preorder = {8,5,7,10,1,12};
        Node root = solution.bstFromPreorder(preorder);
        assertEquals(root.val, 8);
    }

    @Test
    public void test_Solution_5() {
        Solution solution = new Solution();
        int[] preorder = {};
        assertNull(solution.bstFromPreorder(preorder));
    }
}