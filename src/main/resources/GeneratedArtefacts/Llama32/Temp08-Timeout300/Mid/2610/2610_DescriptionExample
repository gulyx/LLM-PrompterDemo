package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertArrayEquals;

public class 2610_DescriptionExampleTest {

    @Test
    public void testMinNumberOfRows() {
        int[] nums = {1,3,4,1,2,3,1};
        int[][] result = minNumberOfRows(nums);
        assertArrayEquals(new int[][]{{1,3,4,2},{1,3},{1}},result);
    }

    @Test
    public void testMinNumberOfRows_2() {
        int[] nums = {1,2,3,4};
        int[][] result = minNumberOfRows(nums);
        assertArrayEquals(new int[][]{{4,3,2,1}},result);
    }

    @Test
    public void testMinNumberOfRows_3() {
        int[] nums = {1,1,1,1,1};
        int[][] result = minNumberOfRows(nums);
        assertArrayEquals(new int[][]{{1}, {1}, {1}, {1}, {1}},result);
    }

    @Test
    public void testMinNumberOfRows_4() {
        int[] nums = {2,2,2,2};
        int[][] result = minNumberOfRows(nums);
        assertArrayEquals(new int[][]{{2,2},{2,2},{2,2}},result);
    }

    @Test
    public void testMinNumberOfRows_5() {
        int[] nums = {};
        int[][] result = minNumberOfRows(nums);
        assertArrayEquals(new int[][]{},result);
    }
}