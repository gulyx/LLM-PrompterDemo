```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class 1381_DescriptionExampleTest {

    @Test
    public void testPush() {
        CustomStack customStack = new CustomStack(3);
        customStack.push(1);
        customStack.push(2);
        assertEquals(-1, customStack.pop());
    }

    @Test
    public void testPop() {
        CustomStack customStack = new CustomStack(3);
        customStack.push(1);
        customStack.push(2);
        customStack.push(3);
        assertEquals(-1, customStack.pop());
    }

    @Test
    public void testIncrement() {
        CustomStack customStack = new CustomStack(3);
        customStack.push(1);
        customStack.push(2);
        customStack.push(3);
        int result = customStack.increment(0, 100);
        assertEquals(-1, result);
    }

    @Test
    public void testIncrementPush() {
        CustomStack customStack = new CustomStack(3);
        customStack.push(1);
        customStack.push(2);
        customStack.push(3);
        int result = customStack.increment(1, 100);
        assertEquals(-1, result);
    }

    @Test
    public void testIncrementPushAll() {
        CustomStack customStack = new CustomStack(3);
        customStack.push(1);
        customStack.push(2);
        customStack.push(3);
        int result = customStack.increment(3, 100);
        assertEquals(-1, result);
    }
}
```