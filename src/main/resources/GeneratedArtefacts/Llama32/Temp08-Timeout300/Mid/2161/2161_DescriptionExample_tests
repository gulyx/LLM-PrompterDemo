```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2161;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2161.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class Solution_testsTest {

    @Test
    public void test_example_1() {
        Solution solution = new Solution();
        int[] nums = {9,12,5,10,14,3,10};
        int pivot = 10;
        int[] result = solution.reorderArray(nums, pivot);
        assertArrayEquals(new int[]{9,5,3,10,10,12,14}, result);
    }

    @Test
    public void test_example_2() {
        Solution solution = new Solution();
        int[] nums = {-3,4,3,2};
        int pivot = 2;
        int[] result = solution.reorderArray(nums, pivot);
        assertArrayEquals(new int[]{-3,2,4,3}, result);
    }

    @Test
    public void test_example_3() {
        Solution solution = new Solution();
        int[] nums = {1,1,2};
        int pivot = 1;
        int[] result = solution.reorderArray(nums, pivot);
        assertArrayEquals(new int[]{1,2,1}, result);
    }

    @Test
    public void test_example_4() {
        Solution solution = new Solution();
        int[] nums = {1,3,2};
        int pivot = 2;
        int[] result = solution.reorderArray(nums, pivot);
        assertArrayEquals(new int[]{1,2,3}, result);
    }

    @Test
    public void test_example_5() {
        Solution solution = new Solution();
        int[] nums = {1,1,1};
        int pivot = 1;
        int[] result = solution.reorderArray(nums, pivot);
        assertArrayEquals(new int[]{1,1,1}, result);
    }
}
```