package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2079;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2079.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;
import static org.junit.jupiter.api.Assertions.assertThrows;

public class 2079_DescriptionOnly_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] plants = {2,3,5,8};
        int capacity = 6;
        assertEquals(10,solution.wateringCanCapacity(plants,capacity));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] plants = {0,1,2,3,4,5};
        int capacity = 5;
        assertEquals(6,solution.wateringCanCapacity(plants,capacity));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] plants = {1,2,3,4,5,0,2};
        int capacity = 2;
        assertEquals(7,solution.wateringCanCapacity(plants,capacity));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] plants = {10000,1,1,1,1,1,13,3,3};
        int capacity = 20;
        assertEquals(8,solution.wateringCanCapacity(plants,capacity));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] plants = {2};
        int capacity = 2;
        assertEquals(2,solution.wateringCanCapacity(plants,capacity));
    }
}