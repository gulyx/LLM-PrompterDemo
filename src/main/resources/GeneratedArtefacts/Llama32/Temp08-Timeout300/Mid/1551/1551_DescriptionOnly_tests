package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1551;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1551.Solution;

public class 1551_DescriptionOnly_testsTest {

    Solution solution = new Solution();

    @Test
    public void test1() {
        int n = 3;
        assertEquals(2, solution.makeArrayEqual(n));
    }

    @Test
    public void test2() {
        int n = 7;
        assertEquals(6, solution.makeArrayEqual(n));
    }

    @Test
    public void test3() {
        int n = 5;
        assertEquals(4, solution.makeArrayEqual(n));
    }

    @Test
    public void test4() {
        int n = 8;
        assertEquals(7, solution.makeArrayEqual(n));
    }

    @Test
    public void test5() {
        int n = 1;
        assertEquals(0, solution.makeArrayEqual(n));
    }
}