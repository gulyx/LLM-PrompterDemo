package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 1441_DescriptionExample_testsTest {

    @Test
    public void example_0() {
        Solution solution = new Solution();
        String[] expected = {"Push","Push","Pop","Push"};
        String[] actual = solution.buildArray(new int[]{1,3}, 3);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void example_1() {
        Solution solution = new Solution();
        String[] expected = {"Push","Push","Push"};
        String[] actual = solution.buildArray(new int[]{1,2,3}, 3);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void example_2() {
        Solution solution = new Solution();
        String[] expected = {"Push","Push"};
        String[] actual = solution.buildArray(new int[]{1,2}, 4);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void example_3() {
        Solution solution = new Solution();
        String[] expected = {"Push","Push"};
        String[] actual = solution.buildArray(new int[]{1,0,0}, 1);
        assertArrayEquals(expected, actual);
    }

    @Test
    public void example_4() {
        Solution solution = new Solution();
        String[] expected = {};
        String[] actual = solution.buildArray(new int[]{1,2,3,9}, 10);
        assertArrayEquals(expected, actual);
    }
}