package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1441.Solution;
import org.junit.jupiter.api.Test;
import java.util.ArrayList;
import static org.junit.jupiter.api.Assertions.*;

public class 1441_DescriptionOnly_testsTest {

    @Test
    void test1() {
        Solution solution = new Solution();
        ArrayList<String> result = solution.buildArray(new int[]{1,2,3}, 3);
        assertEquals("[Push, Push, Pop, Push]",result.get(0));
    }

    @Test
    void test2() {
        Solution solution = new Solution();
        ArrayList<String> result = solution.buildArray(new int[]{1,1,1}, 3);
        assertEquals("[Push, Pop, Push]",result.get(0));
    }

    @Test
    void test3() {
        Solution solution = new Solution();
        ArrayList<String> result = solution.buildArray(new int[]{1,2,3,4,5,6}, 6);
        assertEquals("[Push, Push, Pop, Push, Push, Pop]",result.get(0));
    }

    @Test
    void test4() {
        Solution solution = new Solution();
        ArrayList<String> result = solution.buildArray(new int[]{1,2,3,4,5,6}, 7);
        assertEquals("[Push, Push, Pop, Push, Push, Push]",result.get(0));
    }

    @Test
    void test5() {
        Solution solution = new Solution();
        ArrayList<String> result = solution.buildArray(new int[]{1,2,3,4,5,6}, 8);
        assertEquals("[Push, Push, Pop, Push, Push, Push]",result.get(0));
    }
}