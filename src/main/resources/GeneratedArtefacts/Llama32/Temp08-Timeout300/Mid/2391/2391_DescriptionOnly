package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;
public class 2391_DescriptionOnlyTest {

    @Test
    public void test_0() {
        String[] garbage = new String[]{"PGMP", "PPMGP"};
        int[] travel = new int[]{2, 1};
        assertEquals(12, minCost(garbage, travel));
    }

    @Test
    public void test_1() {
        String[] garbage = new String[]{"GGG","MPMPPMMM","PMMGP","PGPM","G"};
        int[] travel = new int[]{3, 2, 7, 2, 4};
        assertEquals(22, minCost(garbage, travel));
    }

    @Test
    public void test_2() {
        String[] garbage = new String[]{"MPGP", "GGPMG"};
        int[] travel = new int[]{1, 5};
        assertEquals(11, minCost(garbage, travel));
    }

    @Test
    public void test_3() {
        String[] garbage = new String[]{"PGM","PGP","PMMPPMMM","MPGP","G"};
        int[] travel = new int[]{2, 1, 7, 2, 4};
        assertEquals(26, minCost(garbage, travel));
    }

    @Test
    public void test_4() {
        String[] garbage = new String[]{"MPMPPMMM", "PGPM", "G"};
        int[] travel = new int[]{1, 5, 4};
        assertEquals(15, minCost(garbage, travel));
    }
}