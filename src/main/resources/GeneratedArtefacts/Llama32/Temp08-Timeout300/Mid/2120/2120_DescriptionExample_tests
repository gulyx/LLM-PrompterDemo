```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2120;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2120.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class Solution_testsTest {

    @Test
    public void testExample1() {
        int n = 3;
        int[] startPos = {0,1};
        String s = "RRDDLU";
        int[] expected = {1,5,4,3,1,0};
        Solution.Solution solution = new Solution.Solution();
        int[] result = solution.moveZero(n, startPos, s);
        assertArrayEquals(expected, result);
    }

    @Test
    public void testExample2() {
        int n = 2;
        int[] startPos = {1,1};
        String s = "LURD";
        int[] expected = {4,1,0,0};
        Solution.Solution solution = new Solution.Solution();
        int[] result = solution.moveZero(n, startPos, s);
        assertArrayEquals(expected, result);
    }

    @Test
    public void testExample3() {
        int n = 1;
        int[] startPos = {0,0};
        String s = "LRUD";
        int[] expected = {0,0,0,0};
        Solution.Solution solution = new Solution.Solution();
        int[] result = solution.moveZero(n, startPos, s);
        assertArrayEquals(expected, result);
    }

    @Test
    public void testExample4() {
        int n = 5;
        int[] startPos = {2,2};
        String s = "UUURRDDD";
        int[] expected = {2,3,1,0,0};
        Solution.Solution solution = new Solution.Solution();
        int[] result = solution.moveZero(n, startPos, s);
        assertArrayEquals(expected, result);
    }

    @Test
    public void testExample5() {
        int n = 4;
        int[] startPos = {1,3};
        String s = "RUDD";
        int[] expected = {2,0,0,0};
        Solution.Solution solution = new Solution.Solution();
        int[] result = solution.moveZero(n, startPos, s);
        assertArrayEquals(expected, result);
    }
}
```