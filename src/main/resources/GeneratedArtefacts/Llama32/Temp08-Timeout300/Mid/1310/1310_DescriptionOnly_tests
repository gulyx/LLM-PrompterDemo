package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310.Solution;

public class DescriptionOnly_testsTest {

    @org.junit.Test
    public void test1() {
        Solution solution = new Solution();
        int[] arr = {1, 3};
        int[][] queries = {{2},{4}};
        int[] answer = solution.xorQueries(arr, queries);
        org.junit.Assert.assertArrayEquals(new int[]{0,4}, answer);
    }

    @org.junit.Test
    public void test2() {
        Solution solution = new Solution();
        int[] arr = {1, 2, 3};
        int[][] queries = {{1},{3}};
        int[] answer = solution.xorQueries(arr, queries);
        org.junit.Assert.assertArrayEquals(new int[]{1,0}, answer);
    }

    @org.junit.Test
    public void test3() {
        Solution solution = new Solution();
        int[] arr = {2, 7, 4, 10, 23};
        int[][] queries = {{2},{5}};
        int[] answer = solution.xorQueries(arr, queries);
        org.junit.Assert.assertArrayEquals(new int[]{6,18}, answer);
    }

    @org.junit.Test
    public void test4() {
        Solution solution = new Solution();
        int[] arr = {1, 3};
        int[][] queries = {{2},{1}};
        int[] answer = solution.xorQueries(arr, queries);
        org.junit.Assert.assertArrayEquals(new int[]{0,2}, answer);
    }

    @org.junit.Test
    public void test5() {
        Solution solution = new Solution();
        int[] arr = {4, 8};
        int[][] queries = {{4},{7}};
        int[] answer = solution.xorQueries(arr, queries);
        org.junit.Assert.assertArrayEquals(new int[]{0,0}, answer);
    }
}