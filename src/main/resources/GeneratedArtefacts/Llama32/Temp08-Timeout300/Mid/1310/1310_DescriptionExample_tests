```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1310.Solution;
import org.junit.Test;
import static org.junit.Assert.*;
import java.util.Arrays;

public class Solution_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] arr = {1, 3, 4, 8};
        int[][] queries = {{0, 1}, {1, 2}, {0, 3}, {3, 3}};
        int[] result = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{2, 7, 14, 8}, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] arr = {4, 8, 2, 10};
        int[][] queries = {{2, 3}, {1, 3}, {0, 0}, {0, 3}};
        int[] result = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{8, 0, 4, 4}, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] arr = {1, 2};
        int[][] queries = {{0, 1}};
        int[] result = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{1}, result);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] arr = {5, 7, 8};
        int[][] queries = {{0, 1}};
        int[] result = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{6}, result);
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] arr = {1, 2, 3};
        int[][] queries = {{0, 2}};
        int[] result = solution.xorQueries(arr, queries);
        assertArrayEquals(new int[]{4}, result);
    }
}
```