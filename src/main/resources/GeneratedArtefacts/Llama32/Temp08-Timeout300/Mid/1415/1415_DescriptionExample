package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class HappyString {

    @Test
    public void testHappyString_1() {
        int n = 1;
        int k = 3;
        String result = solution(n, k);
        assertEquals("c", result);
    }

    @Test
    public void testHappyString_2() {
        int n = 1;
        int k = 4;
        String result = solution(n, k);
        assertEquals("", result);
    }

    @Test
    public void testHappyString_3() {
        int n = 3;
        int k = 9;
        String result = solution(n, k);
        assertEquals("cab", result);
    }

    @Test
    public void testHappyString_4() {
        int n = 1;
        int k = 1;
        String result = solution(n, k);
        assertEquals("a", result);
    }

    @Test
    public void testHappyString_5() {
        int n = 2;
        int k = 10;
        String result = solution(n, k);
        assertEquals("abcbabcbcb", result);
    }
}