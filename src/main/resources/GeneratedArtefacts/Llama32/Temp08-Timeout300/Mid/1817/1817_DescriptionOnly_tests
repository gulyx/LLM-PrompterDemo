package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1817;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1817.Solution;
import org.junit.Test;
import java.util.HashMap;
import java.util.Map;

public class DescriptionOnlyTestsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[][] logs = {{2,0},{2,1},{1,0}};
        int k = 5;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assert answer[0] == 3;
        assert answer[1] == 2;
        assert answer[2] == 1;
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[][] logs = {{4,0},{4,1},{4,2},{5,0}};
        int k = 7;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assert answer[0] == 3;
        assert answer[1] == 3;
        assert answer[2] == 2;
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[][] logs = {{10,4},{15,5},{20,6}};
        int k = 1000000;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assert answer[0] == 1;
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[][] logs = {{5,2},{8,7},{10,6}};
        int k = 50000000;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assert answer[0] == 1;
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[][] logs = {{7,9},{4,3},{7,3}};
        int k = 10000;
        int[] answer = solution.findDifferentUsersPerMinute(logs, k);
        assert answer[0] == 2;
    }
}