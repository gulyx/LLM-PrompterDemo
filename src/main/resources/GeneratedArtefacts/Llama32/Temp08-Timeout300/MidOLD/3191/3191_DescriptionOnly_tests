package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionOnlyTest {

    @Test
 public void test_0() {
     int[] nums = {0, 1};
     assertEquals(-1, minOperations(nums));
 }

    @Test
 public void test_1() {
     int[] nums = {1, 1, 1, 1};
     assertEquals(0, minOperations(nums));
 }

    @Test
 public void test_2() {
     int[] nums = {1, 0, 1, 0, 1, 0, 1};
     assertEquals(6, minOperations(nums));
 }

    @Test 
    public void test_3() {
        int[] nums = {1, 1, 0, 0, 1, 1};
        assertEquals(10, minOperations(nums));
    }

    @Test
    public void test_4() {
     int[] nums = {1, 0, 1, 1, 0, 1, 1};
     assertEquals(-1, minOperations(nums));
 }
    
 }