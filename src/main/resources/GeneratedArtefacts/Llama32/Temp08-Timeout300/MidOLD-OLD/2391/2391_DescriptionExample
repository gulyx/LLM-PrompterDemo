package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class 2391_DescriptionExample {

    @Test
    public void test_1() {
        String[] garbage = {"G", "P", "GP", "GG"};
        int[] travel = {2, 4, 3};
        assertEquals(21, minGarbage(garbage, travel));
    }

    @Test
    public void test_2() {
        String[] garbage = {"MMM","PGM","GP"};
        int[] travel = {3,10};
        assertEquals(37, minGarbage(garbage, travel));
    }

    @Test
    public void test_3() {
        String[] garbage = {"G", "P", "MPG"};
        int[] travel = {2, 4, 1};
        assertEquals(23, minGarbage(garbage, travel));
    }

    @Test
    public void test_4() {
        String[] garbage = {"MPMG", "PPM", "PG"};
        int[] travel = {3,10,9};
        assertEquals(39, minGarbage(garbage, travel));
    }

    @Test
    public void test_5() {
        String[] garbage = {"GGGP", "PMPG", "PMGP"};
        int[] travel = {2, 4, 1};
        assertEquals(38, minGarbage(garbage, travel));
    }
}