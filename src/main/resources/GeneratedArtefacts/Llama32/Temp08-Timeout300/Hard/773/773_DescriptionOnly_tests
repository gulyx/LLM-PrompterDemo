package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.773.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertFalse;
import static org.junit.Assert.assertTrue;

public class 773_DescriptionOnly_testsTest {
 
    @Test
    public void test1() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{ { 1, 2, 3 }, { 4, 5, 0 } });
        assertEquals(6, result);
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{ { 1, 0, 2 }, { 3, 4, 5 } });
        assertEquals(-1, result);
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{ { 1, 2 }, { 3, 4 } });
        assertFalse(result == -1);
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{ { 1, 0, 2 }, { 3, 0, 5 } });
        assertEquals(7, result);
    }
 
    @Test
    public void test5() {
        Solution solution = new Solution();
        int result = solution.minMovesToMakeBoardBalanced(new int[][]{ { 1, 4, 2 }, { 3, 5, 0 } });
        assertEquals(10, result);
    }
}