package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1944_DescriptionOnly_testsTest {

    @Test
    public void test_canSee_2() {
        Solution solution = new Solution();
        int[] heights = {1,3,2,2,5};
        int expected[] = {2,1,1,0,1};
        int result[] = solution.canSeePeopleHeCanSee(heights);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_canSee_7() {
        Solution solution = new Solution();
        int[] heights = {1,4,3,2,5,8,9};
        int expected[] = {5, 4, 3, 2, 1, 0, 1};
        int result[] = solution.canSeePeopleHeCanSee(heights);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_canSee_0() {
        Solution solution = new Solution();
        int[] heights = {4,3,2,0,5};
        int expected[] = {1, 1, 1, 0, 1};
        int result[] = solution.canSeePeopleHeCanSee(heights);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_canSee_8() {
        Solution solution = new Solution();
        int[] heights = {2,4,3,1,5,6,7,8};
        int expected[] = {7, 6, 5, 4, 3, 2, 1, 0};
        int result[] = solution.canSeePeopleHeCanSee(heights);
        assertArrayEquals(expected, result);
    }

    @Test
    public void test_canSee_1() {
        Solution solution = new Solution();
        int[] heights = {5,4,3,2,1};
        int expected[] = {4, 3, 2, 1, 0};
        int result[] = solution.canSeePeopleHeCanSee(heights);
        assertArrayEquals(expected, result);
    }

}