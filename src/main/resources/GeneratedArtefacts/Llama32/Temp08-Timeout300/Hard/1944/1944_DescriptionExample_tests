package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1944.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class 1944_DescriptionExample_testsTest {

    @Test
    public void test_0() {
        Solution solution = new Solution();
        int[] heights = {10,6,8,5,11,9};
        int[] result = solution.canyouseeheights(heights);
        assertArrayEquals(new int[]{3,1,2,1,1,0},result);
    }

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] heights = {5,1,2,3,10};
        int[] result = solution.canyouseeheights(heights);
        assertArrayEquals(new int[]{4,1,1,1,0},result);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] heights = {1,3,2,4,5};
        int[] result = solution.canyouseeheights(heights);
        assertArrayEquals(new int[]{2,0,1,0,0},result);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] heights = {6,7,4,5,9};
        int[] result = solution.canyouseeheights(heights);
        assertArrayEquals(new int[]{2,1,0,0,1},result);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] heights = {1, 5, 3, 6, 8};
        int[] result = solution.canyouseeheights(heights);
        assertArrayEquals(new int[]{0,2,1,0,0},result);
    }
}
