package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.assertEquals;
import java.util.*;

public class 1579_DescriptionOnlyTest {

    @Test
    public void test1() {
        int[][] edges = {{0,1,2},{1,3,4},{5,6,7}};
        assertEquals(2, maxNumEdgesToRemove(edges));
    }

    @Test
    public void test2() {
        int[][] edges = {{0,1,0},{0,2,1},{0,3,1},{3,4,0},{3,5,0},{2,4,0},{1,5,1},{2,6,1},{4,7,1}};
        assertEquals(5, maxNumEdgesToRemove(edges));
    }

    @Test
    public void test3() {
        int[][] edges = {{0,1,1},{1,2,1},{1,3,1},{2,3,1}};
        assertEquals(-1, maxNumEdgesToRemove(edges));
    }

    @Test
    public void test4() {
        int[][] edges = {{0,1,0},{1,2,0},{2,3,0},{3,4,0}};
        assertEquals(6, maxNumEdgesToRemove(edges));
    }

    @Test
    public void test5() {
        int[][] edges = {};
        assertEquals(-1, maxNumEdgesToRemove(edges));
    }
}

public int maxNumEdgesToRemove(int[][] edges) {

}