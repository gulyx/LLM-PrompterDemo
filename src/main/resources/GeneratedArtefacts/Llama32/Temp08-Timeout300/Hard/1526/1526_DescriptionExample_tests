package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen;

import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExampleTest {

    @Test
    public void test_1() {
        int[] target = {1,2,3,2,1};
        int[] initial = {0,0,0,0,0};
        assertEquals(3, minOperations(target, initial));
    }

    @Test
    public void test_2() {
        int[] target = {3,1,1,2};
        int[] initial = {0,0,0,0};
        assertEquals(4, minOperations(target, initial));
    }

    @Test
    public void test_3() {
        int[] target = {3,1,5,4,2};
        int[] initial = {0,0,0,0,0};
        assertEquals(7, minOperations(target, initial));
    }

    @Test
    public void test_4() {
        int[] target = {1,0,0,0,0};
        int[] initial = {0,0,0,0,0};
        assertEquals(1, minOperations(target, initial));
    }

    @Test
    public void test_5() {
        int[] target = {0,0,0,0,0};
        int[] initial = {0,0,0,0,0};
        assertEquals(0, minOperations(target, initial));
    }
}