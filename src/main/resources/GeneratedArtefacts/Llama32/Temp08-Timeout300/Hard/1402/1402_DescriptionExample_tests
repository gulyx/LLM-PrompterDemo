package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1402;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.1402.Solution;
import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.assertEquals;

public class 1402_DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] satisfaction = {-1,-8,0,5,-9};
        assertEquals(14, solution.maxSatisfaction(satisfaction));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] satisfaction = {4,3,2};
        assertEquals(20, solution.maxSatisfaction(satisfaction));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] satisfaction = {-1,-4,-5};
        assertEquals(0, solution.maxSatisfaction(satisfaction));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] satisfaction = {10, -20, 5, 5, 10};
        assertEquals(-15, solution.maxSatisfaction(satisfaction));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] satisfaction = {-1,-2,-3};
        assertEquals(0, solution.maxSatisfaction(satisfaction));
    }
}