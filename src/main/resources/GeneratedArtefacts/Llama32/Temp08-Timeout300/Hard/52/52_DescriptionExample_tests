package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.52;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.52.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class 52_DescriptionExample_testsTest {

    @Test
    public void testNumberOfDistinctSolutionsForN4() {
        Solution solution = new Solution();
        assertEquals(2, solution.totalNQueens(4));
    }

    @Test
    public void testNumberOfDistinctSolutionsForN1() {
        Solution solution = new Solution();
        assertEquals(1, solution.totalNQueens(1));
    }

    @Test
    public void testNumberOfDistinctSolutionsForN2() {
        Solution solution = new Solution();
        assertEquals(0, solution.totalNQueens(2));
    }

    @Test
    public void testNumberOfDistinctSolutionsForN3() {
        Solution solution = new Solution();
        assertEquals(4, solution.totalNQueens(3));
    }

    @Test
    public void testNumberOfDistinctSolutionsForN8() {
        Solution solution = new Solution();
        assertEquals(92, solution.totalNQueens(8));
    }
}
