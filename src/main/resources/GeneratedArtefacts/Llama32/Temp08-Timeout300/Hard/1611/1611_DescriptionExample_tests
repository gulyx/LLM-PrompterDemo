public class DescriptionExampleTests {

    @Test
    public void test_0_to_3() {
        Solution solution = new Solution();
        int actual = solution.minimumOperations(3);
        assertEquals(2, actual);
    }

    @Test
    public void test_4_to_6() {
        Solution solution = new Solution();
        int actual = solution.minimumOperations(6);
        assertEquals(4, actual);
    }

    @Test
    public void test_7_to_8() {
        Solution solution = new Solution();
        int actual = solution.minimumOperations(8);
        assertEquals(1, actual);
    }

    @Test
    public void test_10_to_11() {
        Solution solution = new Solution();
        int actual = solution.minimumOperations(11);
        assertEquals(3, actual);
    }

    @Test
    public void test_15_to_16() {
        Solution solution = new Solution();
        int actual = solution.minimumOperations(16);
        assertEquals(1, actual);
    }
}