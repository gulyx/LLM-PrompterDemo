package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2551;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.2551.Solution;
import org.junit.Test;
import static org.junit.Assert.*;

public class DescriptionExample_testsTest {

    @Test
    public void test1() {
        Solution solution = new Solution();
        int[] weights = {1,3,5,1};
        int k = 2;
        int expectedOutput = 4;
        assertEquals(expectedOutput, solution.scoreOfKales(weights, k));
    }

    @Test
    public void test2() {
        Solution solution = new Solution();
        int[] weights = {1, 3};
        int k = 2;
        int expectedOutput = 0;
        assertEquals(expectedOutput, solution.scoreOfKales(weights, k));
    }

    @Test
    public void test3() {
        Solution solution = new Solution();
        int[] weights = {4,6,8,1,7};
        int k = 5;
        long expectedOutput = -6L;
        assertEquals(expectedOutput, solution.scoreOfKales(weights, k));
    }

    @Test
    public void test4() {
        Solution solution = new Solution();
        int[] weights = {10,3};
        int k = 2;
        int expectedOutput = 8;
        assertEquals(expectedOutput, solution.scoreOfKales(weights, k));
    }

    @Test
    public void test5() {
        Solution solution = new Solution();
        int[] weights = {1,9,6,7};
        int k = 3;
        long expectedOutput = -15L;
        assertEquals(expectedOutput, solution.scoreOfKales(weights, k));
    }
}