```java
package it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.732;

import it.cnr.iasi.saks.llmPrompter.leetCodeDemo.gen.732.Solution;
import org.junit.Test;
import static org.junit.Assert.assertEquals;

public class SolutionTest {

    @Test
    public void test_1() {
        Solution solution = new Solution();
        int[] result = solution.book(new int[][]{{10, 20}, {50, 60}, {10, 40}, {5, 15}, {5, 10}, {25, 55}});
        assertEquals(3, result[0]);
    }

    @Test
    public void test_2() {
        Solution solution = new Solution();
        int[] result = solution.book(new int[][]{{1, 2}, {2, 3}, {3, 4}});
        assertEquals(1, result[0]);
    }

    @Test
    public void test_3() {
        Solution solution = new Solution();
        int[] result = solution.book(new int[][]{{1, 2}, {1, 3}});
        assertEquals(0, result[0]);
    }

    @Test
    public void test_4() {
        Solution solution = new Solution();
        int[] result = solution.book(new int[][]{{1, 2}, {2, 3}});
        assertEquals(1, result[0]);
    }

    @Test
    public void test_5() {
        Solution solution = new Solution();
        int[] result = solution.book(new int[][]{});
        assertEquals(0, result[0]);
    }
}
```